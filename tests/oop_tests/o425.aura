// OOP: Static Factory Pattern
class ConcreteC22880Eo {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ConcreteC22880Eo {
        return ConcreteC22880Eo(value: 42);
    }
    
    public static def create_custom(val: int) -> ConcreteC22880Eo {
        return ConcreteC22880Eo(value: val);
    }
}

let obj1 = ConcreteC22880Eo.create_default();
let obj2 = ConcreteC22880Eo.create_custom(7);

print(obj1.get_value());
print(obj2.get_value());
