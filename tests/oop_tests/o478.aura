// OOP: Static Members
class BaseC41938Iy {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC41938Iy.instance_count = BaseC41938Iy.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC41938Iy.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC41938Iy(id: 1);
let obj2 = BaseC41938Iy(id: 2);
let obj3 = BaseC41938Iy(id: 3);

print(BaseC41938Iy.instance_count);
print(obj1.get_id());
print(obj3.get_id());
