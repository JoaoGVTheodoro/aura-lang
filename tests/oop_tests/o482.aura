// OOP: Builder Pattern
class ConcreteC15840BoBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ConcreteC15840BoBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ConcreteC15840BoBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ConcreteC15840Bo {
        return ConcreteC15840Bo(name: self.name, age: self.age, email: self.email);
    }
}

class ConcreteC15840Bo {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ConcreteC15840BoBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
