// OOP: Static Factory Pattern
class C29556Xq {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C29556Xq {
        return C29556Xq(value: 42);
    }
    
    public static def create_custom(val: int) -> C29556Xq {
        return C29556Xq(value: val);
    }
}

let obj1 = C29556Xq.create_default();
let obj2 = C29556Xq.create_custom(4);

print(obj1.get_value());
print(obj2.get_value());
