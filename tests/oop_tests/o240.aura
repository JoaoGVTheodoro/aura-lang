// OOP: Static Members
class ConcreteC56710Kl {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC56710Kl.instance_count = ConcreteC56710Kl.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC56710Kl.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC56710Kl(id: 1);
let obj2 = ConcreteC56710Kl(id: 2);
let obj3 = ConcreteC56710Kl(id: 3);

print(ConcreteC56710Kl.instance_count);
print(obj1.get_id());
print(obj3.get_id());
