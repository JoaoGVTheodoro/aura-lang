// OOP: Builder Pattern
class C93522HhBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> C93522HhBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> C93522HhBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> C93522Hh {
        return C93522Hh(name: self.name, age: self.age, email: self.email);
    }
}

class C93522Hh {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = C93522HhBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
