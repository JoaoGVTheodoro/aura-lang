// OOP: Static Factory Pattern
class C51901Yt {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C51901Yt {
        return C51901Yt(value: 42);
    }
    
    public static def create_custom(val: int) -> C51901Yt {
        return C51901Yt(value: val);
    }
}

let obj1 = C51901Yt.create_default();
let obj2 = C51901Yt.create_custom(48);

print(obj1.get_value());
print(obj2.get_value());
