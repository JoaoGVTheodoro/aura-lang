// OOP: Static Members
class BaseC10597Vm {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC10597Vm.instance_count = BaseC10597Vm.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC10597Vm.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC10597Vm(id: 1);
let obj2 = BaseC10597Vm(id: 2);
let obj3 = BaseC10597Vm(id: 3);

print(BaseC10597Vm.instance_count);
print(obj1.get_id());
print(obj3.get_id());
