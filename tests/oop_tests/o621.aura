// OOP: Static Factory Pattern
class AbstractC31405Nv {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC31405Nv {
        return AbstractC31405Nv(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC31405Nv {
        return AbstractC31405Nv(value: val);
    }
}

let obj1 = AbstractC31405Nv.create_default();
let obj2 = AbstractC31405Nv.create_custom(28);

print(obj1.get_value());
print(obj2.get_value());
