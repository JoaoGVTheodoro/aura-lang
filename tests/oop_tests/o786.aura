// OOP: Static Members
class MockC56341Zg {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC56341Zg.instance_count = MockC56341Zg.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC56341Zg.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC56341Zg(id: 1);
let obj2 = MockC56341Zg(id: 2);
let obj3 = MockC56341Zg(id: 3);

print(MockC56341Zg.instance_count);
print(obj1.get_id());
print(obj3.get_id());
