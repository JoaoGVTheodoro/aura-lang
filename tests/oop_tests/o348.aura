// OOP: Delegation
class BaseC41484Ye {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ConcreteC38565Lk {
    private let delegate: BaseC41484Ye
    
    def init(t: BaseC41484Ye) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC41484Ye();
target_obj.data = 25;
let delegator_obj = ConcreteC38565Lk(t: target_obj);
print(delegator_obj.process());
