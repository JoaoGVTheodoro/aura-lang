// OOP: Deep Inheritance Chain (3 levels)
class AbstractC25425Ew {
    protected let base_val: int = 40
    public def get_val() -> int { return self.base_val; }
}

class BaseC87300Vz extends AbstractC25425Ew {
    public def get_val() -> int { return self.base_val * 2; }
}

class RealC98861He extends BaseC87300Vz {
    public def get_val() -> int { return self.base_val * 3; }
}

let obj = RealC98861He(base_val: 40);
print(obj.get_val());
