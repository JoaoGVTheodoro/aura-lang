// OOP: Deep Inheritance Chain (4 levels)
class RealC14119Kc {
    protected let base_val: int = 22
    public def get_val() -> int { return self.base_val; }
}

class ImplC32075Zv extends RealC14119Kc {
    public def get_val() -> int { return self.base_val * 2; }
}

class ConcreteC83335Gz extends ImplC32075Zv {
    public def get_val() -> int { return self.base_val * 3; }
}

class AbstractC65075Tz extends ConcreteC83335Gz {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = AbstractC65075Tz(base_val: 22);
print(obj.get_val());
