// OOP: Delegation
class ConcreteC89540Mc {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ConcreteC37499Zx {
    private let delegate: ConcreteC89540Mc
    
    def init(t: ConcreteC89540Mc) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC89540Mc();
target_obj.data = 25;
let delegator_obj = ConcreteC37499Zx(t: target_obj);
print(delegator_obj.process());
