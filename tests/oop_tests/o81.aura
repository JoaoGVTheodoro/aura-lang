// OOP: Composition
class C37306Hq {
    public let data: int = 50
    public def process() -> int { return self.data * 2; }
}

class ConcreteC59163Qa {
    private let component: C37306Hq
    
    def init(comp: C37306Hq) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C37306Hq(data: 50);
let outer_obj = ConcreteC59163Qa(comp: inner_obj);
print(outer_obj.transform());
