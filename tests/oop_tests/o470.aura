// OOP: Mixin-like Composition
class MockC32312Cq {
    public def log(msg: str) { print(msg); }
}

class BaseC63567Ah {
    private let logger: MockC32312Cq
    public let value: int
    
    def init(val: int, logger: MockC32312Cq) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = MockC32312Cq();
let obj = BaseC63567Ah(val: 50, logger: logger);
print(obj.process());
