// OOP: Mixin-like Composition
class ConcreteC42682Yw {
    public def log(msg: str) { print(msg); }
}

class RealC34383Av {
    private let logger: ConcreteC42682Yw
    public let value: int
    
    def init(val: int, logger: ConcreteC42682Yw) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC42682Yw();
let obj = RealC34383Av(val: 50, logger: logger);
print(obj.process());
