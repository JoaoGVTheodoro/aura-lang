// OOP: Composition
class RealC35402Ad {
    public let data: int = 30
    public def process() -> int { return self.data * 2; }
}

class BaseC70642Sm {
    private let component: RealC35402Ad
    
    def init(comp: RealC35402Ad) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC35402Ad(data: 30);
let outer_obj = BaseC70642Sm(comp: inner_obj);
print(outer_obj.transform());
