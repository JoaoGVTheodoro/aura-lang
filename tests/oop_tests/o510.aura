// OOP: Builder Pattern
class ConcreteC58789OrBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ConcreteC58789OrBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ConcreteC58789OrBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ConcreteC58789Or {
        return ConcreteC58789Or(name: self.name, age: self.age, email: self.email);
    }
}

class ConcreteC58789Or {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ConcreteC58789OrBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
