// OOP: Delegation
class ImplC14747Rh {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ImplC40101Fv {
    private let delegate: ImplC14747Rh
    
    def init(t: ImplC14747Rh) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ImplC14747Rh();
target_obj.data = 25;
let delegator_obj = ImplC40101Fv(t: target_obj);
print(delegator_obj.process());
