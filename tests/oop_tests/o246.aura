// OOP: Mixin-like Composition
class RealC86956Qb {
    public def log(msg: str) { print(msg); }
}

class BaseC84416Kh {
    private let logger: RealC86956Qb
    public let value: int
    
    def init(val: int, logger: RealC86956Qb) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC86956Qb();
let obj = BaseC84416Kh(val: 50, logger: logger);
print(obj.process());
