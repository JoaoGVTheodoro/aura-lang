// OOP: Static Factory Pattern
class MockC57142Xn {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC57142Xn {
        return MockC57142Xn(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC57142Xn {
        return MockC57142Xn(value: val);
    }
}

let obj1 = MockC57142Xn.create_default();
let obj2 = MockC57142Xn.create_custom(61);

print(obj1.get_value());
print(obj2.get_value());
