// OOP: Mixin-like Composition
class ConcreteC19987Rm {
    public def log(msg: str) { print(msg); }
}

class BaseC63435Pi {
    private let logger: ConcreteC19987Rm
    public let value: int
    
    def init(val: int, logger: ConcreteC19987Rm) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC19987Rm();
let obj = BaseC63435Pi(val: 50, logger: logger);
print(obj.process());
