    protected let base_val: int = 16
    public def get_val() -> int { return self.base_val; }
}

class C35514Ib extends AbstractC64562Qa {
    public def get_val() -> int { return self.base_val * 2; }
}

class RealC76180Se extends C35514Ib {
    public def get_val() -> int { return self.base_val * 3; }
}

class ConcreteC44927Cs extends RealC76180Se {
    public def get_val() -> int { return self.base_val * 4; }
}

