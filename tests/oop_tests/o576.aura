// OOP: Static Members
class AbstractC73670Tt {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC73670Tt.instance_count = AbstractC73670Tt.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC73670Tt.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC73670Tt(id: 1);
let obj2 = AbstractC73670Tt(id: 2);
let obj3 = AbstractC73670Tt(id: 3);

print(AbstractC73670Tt.instance_count);
print(obj1.get_id());
print(obj3.get_id());
