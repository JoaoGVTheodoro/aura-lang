// OOP: Static Factory Pattern
class C68299Sp {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C68299Sp {
        return C68299Sp(value: 42);
    }
    
    public static def create_custom(val: int) -> C68299Sp {
        return C68299Sp(value: val);
    }
}

let obj1 = C68299Sp.create_default();
let obj2 = C68299Sp.create_custom(66);

print(obj1.get_value());
print(obj2.get_value());
