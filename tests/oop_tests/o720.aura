// OOP: Builder Pattern
class MockC40695BaBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> MockC40695BaBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> MockC40695BaBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> MockC40695Ba {
        return MockC40695Ba(name: self.name, age: self.age, email: self.email);
    }
}

class MockC40695Ba {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = MockC40695BaBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
