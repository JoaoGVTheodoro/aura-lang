// OOP: Mixin-like Composition
class ImplC29867Dk {
    public def log(msg: str) { print(msg); }
}

class ConcreteC12457Wx {
    private let logger: ImplC29867Dk
    public let value: int
    
    def init(val: int, logger: ImplC29867Dk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC29867Dk();
let obj = ConcreteC12457Wx(val: 50, logger: logger);
print(obj.process());
