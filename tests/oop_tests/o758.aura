// OOP: Static Members
class BaseC79944We {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC79944We.instance_count = BaseC79944We.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC79944We.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC79944We(id: 1);
let obj2 = BaseC79944We(id: 2);
let obj3 = BaseC79944We(id: 3);

print(BaseC79944We.instance_count);
print(obj1.get_id());
print(obj3.get_id());
