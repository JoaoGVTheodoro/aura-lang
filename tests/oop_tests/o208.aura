// OOP: Delegation
class ConcreteC14704Vp {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class RealC28503Tk {
    private let delegate: ConcreteC14704Vp
    
    def init(t: ConcreteC14704Vp) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC14704Vp();
target_obj.data = 25;
let delegator_obj = RealC28503Tk(t: target_obj);
print(delegator_obj.process());
