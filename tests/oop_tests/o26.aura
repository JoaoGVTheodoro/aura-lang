// OOP: Delegation
class BaseC37609On {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class AbstractC97831Uv {
    private let delegate: BaseC37609On
    
    def init(t: BaseC37609On) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC37609On();
target_obj.data = 25;
let delegator_obj = AbstractC97831Uv(t: target_obj);
print(delegator_obj.process());
