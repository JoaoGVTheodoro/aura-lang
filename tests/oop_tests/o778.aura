// OOP: Mixin-like Composition
class MockC92300Qx {
    public def log(msg: str) { print(msg); }
}

class MockC93016Bg {
    private let logger: MockC92300Qx
    public let value: int
    
    def init(val: int, logger: MockC92300Qx) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = MockC92300Qx();
let obj = MockC93016Bg(val: 50, logger: logger);
print(obj.process());
