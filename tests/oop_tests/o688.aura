// OOP: Static Members
class AbstractC34192Lt {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC34192Lt.instance_count = AbstractC34192Lt.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC34192Lt.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC34192Lt(id: 1);
let obj2 = AbstractC34192Lt(id: 2);
let obj3 = AbstractC34192Lt(id: 3);

print(AbstractC34192Lt.instance_count);
print(obj1.get_id());
print(obj3.get_id());
