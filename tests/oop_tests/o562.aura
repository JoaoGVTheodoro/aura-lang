// OOP: Static Members
class C89564Uq {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C89564Uq.instance_count = C89564Uq.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C89564Uq.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C89564Uq(id: 1);
let obj2 = C89564Uq(id: 2);
let obj3 = C89564Uq(id: 3);

print(C89564Uq.instance_count);
print(obj1.get_id());
print(obj3.get_id());
