// OOP: Mixin-like Composition
class AbstractC93640Hr {
    public def log(msg: str) { print(msg); }
}

class C13945Oa {
    private let logger: AbstractC93640Hr
    public let value: int
    
    def init(val: int, logger: AbstractC93640Hr) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = AbstractC93640Hr();
let obj = C13945Oa(val: 50, logger: logger);
print(obj.process());
