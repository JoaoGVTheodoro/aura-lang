// OOP: Static Factory Pattern
class BaseC25763Qg {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC25763Qg {
        return BaseC25763Qg(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC25763Qg {
        return BaseC25763Qg(value: val);
    }
}

let obj1 = BaseC25763Qg.create_default();
let obj2 = BaseC25763Qg.create_custom(76);

print(obj1.get_value());
print(obj2.get_value());
