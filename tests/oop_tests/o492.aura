// OOP: Static Members
class MockC78219Bh {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC78219Bh.instance_count = MockC78219Bh.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC78219Bh.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC78219Bh(id: 1);
let obj2 = MockC78219Bh(id: 2);
let obj3 = MockC78219Bh(id: 3);

print(MockC78219Bh.instance_count);
print(obj1.get_id());
print(obj3.get_id());
