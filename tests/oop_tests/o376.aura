// OOP: Delegation
class ConcreteC91918Gb {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ConcreteC75509Wc {
    private let delegate: ConcreteC91918Gb
    
    def init(t: ConcreteC91918Gb) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC91918Gb();
target_obj.data = 25;
let delegator_obj = ConcreteC75509Wc(t: target_obj);
print(delegator_obj.process());
