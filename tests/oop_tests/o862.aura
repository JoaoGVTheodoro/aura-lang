// OOP: Mixin-like Composition
class ImplC26406Lg {
    public def log(msg: str) { print(msg); }
}

class BaseC96798Kh {
    private let logger: ImplC26406Lg
    public let value: int
    
    def init(val: int, logger: ImplC26406Lg) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC26406Lg();
let obj = BaseC96798Kh(val: 50, logger: logger);
print(obj.process());
