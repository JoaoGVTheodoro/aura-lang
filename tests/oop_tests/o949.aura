// OOP: Composition
class AbstractC53770De {
    public let data: int = 80
    public def process() -> int { return self.data * 2; }
}

class RealC82126Pa {
    private let component: AbstractC53770De
    
    def init(comp: AbstractC53770De) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = AbstractC53770De(data: 80);
let outer_obj = RealC82126Pa(comp: inner_obj);
print(outer_obj.transform());
