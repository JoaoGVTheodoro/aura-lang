// OOP: Static Members
class ConcreteC78095Nx {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC78095Nx.instance_count = ConcreteC78095Nx.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC78095Nx.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC78095Nx(id: 1);
let obj2 = ConcreteC78095Nx(id: 2);
let obj3 = ConcreteC78095Nx(id: 3);

print(ConcreteC78095Nx.instance_count);
print(obj1.get_id());
print(obj3.get_id());
