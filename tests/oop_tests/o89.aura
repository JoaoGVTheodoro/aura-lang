// OOP: Static Factory Pattern
class RealC43178Gc {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC43178Gc {
        return RealC43178Gc(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC43178Gc {
        return RealC43178Gc(value: val);
    }
}

let obj1 = RealC43178Gc.create_default();
let obj2 = RealC43178Gc.create_custom(84);

print(obj1.get_value());
print(obj2.get_value());
