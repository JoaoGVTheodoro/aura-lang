// OOP: Static Factory Pattern
class ImplC36538Kr {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC36538Kr {
        return ImplC36538Kr(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC36538Kr {
        return ImplC36538Kr(value: val);
    }
}

let obj1 = ImplC36538Kr.create_default();
let obj2 = ImplC36538Kr.create_custom(1);

print(obj1.get_value());
print(obj2.get_value());
