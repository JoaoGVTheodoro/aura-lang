// OOP: Static Members
class ImplC98035Vn {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ImplC98035Vn.instance_count = ImplC98035Vn.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ImplC98035Vn.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ImplC98035Vn(id: 1);
let obj2 = ImplC98035Vn(id: 2);
let obj3 = ImplC98035Vn(id: 3);

print(ImplC98035Vn.instance_count);
print(obj1.get_id());
print(obj3.get_id());
