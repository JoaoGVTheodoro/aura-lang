// OOP: Static Factory Pattern
class RealC83925Vt {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC83925Vt {
        return RealC83925Vt(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC83925Vt {
        return RealC83925Vt(value: val);
    }
}

let obj1 = RealC83925Vt.create_default();
let obj2 = RealC83925Vt.create_custom(99);

print(obj1.get_value());
print(obj2.get_value());
