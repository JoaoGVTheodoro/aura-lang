// OOP: Static Members
class AbstractC24563Up {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC24563Up.instance_count = AbstractC24563Up.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC24563Up.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC24563Up(id: 1);
let obj2 = AbstractC24563Up(id: 2);
let obj3 = AbstractC24563Up(id: 3);

print(AbstractC24563Up.instance_count);
print(obj1.get_id());
print(obj3.get_id());
