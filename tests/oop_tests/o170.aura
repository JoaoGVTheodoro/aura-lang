// OOP: Static Members
class AbstractC40686Md {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC40686Md.instance_count = AbstractC40686Md.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC40686Md.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC40686Md(id: 1);
let obj2 = AbstractC40686Md(id: 2);
let obj3 = AbstractC40686Md(id: 3);

print(AbstractC40686Md.instance_count);
print(obj1.get_id());
print(obj3.get_id());
