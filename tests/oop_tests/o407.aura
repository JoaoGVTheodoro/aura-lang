// OOP: Deep Inheritance Chain (4 levels)
class ImplC63542Qw {
    protected let base_val: int = 32
    public def get_val() -> int { return self.base_val; }
}

class ConcreteC57353Wu extends ImplC63542Qw {
    public def get_val() -> int { return self.base_val * 2; }
}

class BaseC44384Vs extends ConcreteC57353Wu {
    public def get_val() -> int { return self.base_val * 3; }
}

class BaseC99011Gy extends BaseC44384Vs {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = BaseC99011Gy(base_val: 32);
print(obj.get_val());
