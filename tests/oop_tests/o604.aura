// OOP: Static Members
class MockC12483Ig {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC12483Ig.instance_count = MockC12483Ig.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC12483Ig.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC12483Ig(id: 1);
let obj2 = MockC12483Ig(id: 2);
let obj3 = MockC12483Ig(id: 3);

print(MockC12483Ig.instance_count);
print(obj1.get_id());
print(obj3.get_id());
