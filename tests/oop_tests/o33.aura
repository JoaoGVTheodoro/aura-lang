// OOP: Static Factory Pattern
class BaseC49417Cy {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC49417Cy {
        return BaseC49417Cy(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC49417Cy {
        return BaseC49417Cy(value: val);
    }
}

let obj1 = BaseC49417Cy.create_default();
let obj2 = BaseC49417Cy.create_custom(52);

print(obj1.get_value());
print(obj2.get_value());
