// OOP: Composition
class BaseC68344Ry {
    public let data: int = 10
    public def process() -> int { return self.data * 2; }
}

class AbstractC45644Vw {
    private let component: BaseC68344Ry
    
    def init(comp: BaseC68344Ry) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC68344Ry(data: 10);
let outer_obj = AbstractC45644Vw(comp: inner_obj);
print(outer_obj.transform());
