// OOP: Static Factory Pattern
class ConcreteC19246Tj {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ConcreteC19246Tj {
        return ConcreteC19246Tj(value: 42);
    }
    
    public static def create_custom(val: int) -> ConcreteC19246Tj {
        return ConcreteC19246Tj(value: val);
    }
}

let obj1 = ConcreteC19246Tj.create_default();
let obj2 = ConcreteC19246Tj.create_custom(74);

print(obj1.get_value());
print(obj2.get_value());
