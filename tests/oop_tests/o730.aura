// OOP: Static Members
class BaseC79173An {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC79173An.instance_count = BaseC79173An.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC79173An.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC79173An(id: 1);
let obj2 = BaseC79173An(id: 2);
let obj3 = BaseC79173An(id: 3);

print(BaseC79173An.instance_count);
print(obj1.get_id());
print(obj3.get_id());
