// OOP: Static Members
class MockC13229Rx {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC13229Rx.instance_count = MockC13229Rx.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC13229Rx.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC13229Rx(id: 1);
let obj2 = MockC13229Rx(id: 2);
let obj3 = MockC13229Rx(id: 3);

print(MockC13229Rx.instance_count);
print(obj1.get_id());
print(obj3.get_id());
