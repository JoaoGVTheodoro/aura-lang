// OOP: Simple Inheritance
class AbstractC13813Yo {
    let value: int = 54
    public def get_value() -> int { return self.value; }
    public def double_value() -> int { return self.value * 2; }
}

class AbstractC74082Xz extends AbstractC13813Yo {
    public def double_value() -> int { return self.value * 3; }
}

let obj = AbstractC74082Xz(value: 54);
print(obj.get_value());
print(obj.double_value());
