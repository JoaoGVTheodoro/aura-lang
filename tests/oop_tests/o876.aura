// OOP: Mixin-like Composition
class ImplC12443Fw {
    public def log(msg: str) { print(msg); }
}

class C48515Jo {
    private let logger: ImplC12443Fw
    public let value: int
    
    def init(val: int, logger: ImplC12443Fw) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC12443Fw();
let obj = C48515Jo(val: 50, logger: logger);
print(obj.process());
