// OOP: Mixin-like Composition
class BaseC59753Gp {
    public def log(msg: str) { print(msg); }
}

class BaseC14904On {
    private let logger: BaseC59753Gp
    public let value: int
    
    def init(val: int, logger: BaseC59753Gp) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC59753Gp();
let obj = BaseC14904On(val: 50, logger: logger);
print(obj.process());
