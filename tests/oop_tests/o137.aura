// OOP: Composition
class C87947Vv {
    public let data: int = 94
    public def process() -> int { return self.data * 2; }
}

class C40156Lh {
    private let component: C87947Vv
    
    def init(comp: C87947Vv) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C87947Vv(data: 94);
let outer_obj = C40156Lh(comp: inner_obj);
print(outer_obj.transform());
