// OOP: Mixin-like Composition
class RealC23264Xw {
    public def log(msg: str) { print(msg); }
}

class AbstractC18566Bg {
    private let logger: RealC23264Xw
    public let value: int
    
    def init(val: int, logger: RealC23264Xw) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC23264Xw();
let obj = AbstractC18566Bg(val: 50, logger: logger);
print(obj.process());
