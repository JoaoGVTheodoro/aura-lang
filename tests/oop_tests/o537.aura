// OOP: Static Factory Pattern
class C62695Le {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C62695Le {
        return C62695Le(value: 42);
    }
    
    public static def create_custom(val: int) -> C62695Le {
        return C62695Le(value: val);
    }
}

let obj1 = C62695Le.create_default();
let obj2 = C62695Le.create_custom(58);

print(obj1.get_value());
print(obj2.get_value());
