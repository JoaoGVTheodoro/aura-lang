// OOP: Static Members
class BaseC98947Ob {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC98947Ob.instance_count = BaseC98947Ob.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC98947Ob.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC98947Ob(id: 1);
let obj2 = BaseC98947Ob(id: 2);
let obj3 = BaseC98947Ob(id: 3);

print(BaseC98947Ob.instance_count);
print(obj1.get_id());
print(obj3.get_id());
