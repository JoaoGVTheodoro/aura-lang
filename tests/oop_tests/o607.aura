// OOP: Static Factory Pattern
class RealC25597Qp {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC25597Qp {
        return RealC25597Qp(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC25597Qp {
        return RealC25597Qp(value: val);
    }
}

let obj1 = RealC25597Qp.create_default();
let obj2 = RealC25597Qp.create_custom(28);

print(obj1.get_value());
print(obj2.get_value());
