// OOP: Static Factory Pattern
class ImplC10192Pc {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC10192Pc {
        return ImplC10192Pc(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC10192Pc {
        return ImplC10192Pc(value: val);
    }
}

let obj1 = ImplC10192Pc.create_default();
let obj2 = ImplC10192Pc.create_custom(17);

print(obj1.get_value());
print(obj2.get_value());
