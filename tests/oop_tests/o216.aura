// OOP: Builder Pattern
class ImplC60004TtBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ImplC60004TtBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ImplC60004TtBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ImplC60004Tt {
        return ImplC60004Tt(name: self.name, age: self.age, email: self.email);
    }
}

class ImplC60004Tt {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ImplC60004TtBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
