// OOP: Composition
class MockC87268Yf {
    public let data: int = 12
    public def process() -> int { return self.data * 2; }
}

class RealC72799Ok {
    private let component: MockC87268Yf
    
    def init(comp: MockC87268Yf) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC87268Yf(data: 12);
let outer_obj = RealC72799Ok(comp: inner_obj);
print(outer_obj.transform());
