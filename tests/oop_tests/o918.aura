// OOP: Mixin-like Composition
class BaseC35285Sa {
    public def log(msg: str) { print(msg); }
}

class MockC61286Ll {
    private let logger: BaseC35285Sa
    public let value: int
    
    def init(val: int, logger: BaseC35285Sa) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC35285Sa();
let obj = MockC61286Ll(val: 50, logger: logger);
print(obj.process());
