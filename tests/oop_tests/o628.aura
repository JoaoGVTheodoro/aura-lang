// OOP: Delegation
class BaseC17853Wb {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class MockC86999Jh {
    private let delegate: BaseC17853Wb
    
    def init(t: BaseC17853Wb) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC17853Wb();
target_obj.data = 25;
let delegator_obj = MockC86999Jh(t: target_obj);
print(delegator_obj.process());
