// OOP: Mixin-like Composition
class ConcreteC30562Fw {
    public def log(msg: str) { print(msg); }
}

class ImplC54415Di {
    private let logger: ConcreteC30562Fw
    public let value: int
    
    def init(val: int, logger: ConcreteC30562Fw) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC30562Fw();
let obj = ImplC54415Di(val: 50, logger: logger);
print(obj.process());
