// OOP: Composition
class ConcreteC94444Ki {
    public let data: int = 45
    public def process() -> int { return self.data * 2; }
}

class RealC20274Cj {
    private let component: ConcreteC94444Ki
    
    def init(comp: ConcreteC94444Ki) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ConcreteC94444Ki(data: 45);
let outer_obj = RealC20274Cj(comp: inner_obj);
print(outer_obj.transform());
