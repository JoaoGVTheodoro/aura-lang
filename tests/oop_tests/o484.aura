// OOP: Mixin-like Composition
class C65075Ns {
    public def log(msg: str) { print(msg); }
}

class C59846Xa {
    private let logger: C65075Ns
    public let value: int
    
    def init(val: int, logger: C65075Ns) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C65075Ns();
let obj = C59846Xa(val: 50, logger: logger);
print(obj.process());
