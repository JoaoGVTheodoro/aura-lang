// OOP: Composition
class ConcreteC15175Aa {
    public let data: int = 47
    public def process() -> int { return self.data * 2; }
}

class BaseC54821Bz {
    private let component: ConcreteC15175Aa
    
    def init(comp: ConcreteC15175Aa) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ConcreteC15175Aa(data: 47);
let outer_obj = BaseC54821Bz(comp: inner_obj);
print(outer_obj.transform());
