// OOP: Mixin-like Composition
class RealC23046Yh {
    public def log(msg: str) { print(msg); }
}

class MockC48886Vl {
    private let logger: RealC23046Yh
    public let value: int
    
    def init(val: int, logger: RealC23046Yh) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC23046Yh();
let obj = MockC48886Vl(val: 50, logger: logger);
print(obj.process());
