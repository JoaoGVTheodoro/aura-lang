// OOP: Composition
class RealC60793Be {
    public let data: int = 81
    public def process() -> int { return self.data * 2; }
}

class C55982Yh {
    private let component: RealC60793Be
    
    def init(comp: RealC60793Be) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC60793Be(data: 81);
let outer_obj = C55982Yh(comp: inner_obj);
print(outer_obj.transform());
