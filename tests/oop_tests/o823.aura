// OOP: Composition
class MockC43728Lb {
    public let data: int = 3
    public def process() -> int { return self.data * 2; }
}

class RealC51803Jb {
    private let component: MockC43728Lb
    
    def init(comp: MockC43728Lb) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC43728Lb(data: 3);
let outer_obj = RealC51803Jb(comp: inner_obj);
print(outer_obj.transform());
