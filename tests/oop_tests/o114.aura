// OOP: Static Members
class C78867Pr {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C78867Pr.instance_count = C78867Pr.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C78867Pr.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C78867Pr(id: 1);
let obj2 = C78867Pr(id: 2);
let obj3 = C78867Pr(id: 3);

print(C78867Pr.instance_count);
print(obj1.get_id());
print(obj3.get_id());
