// OOP: Static Factory Pattern
class RealC39550Ga {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC39550Ga {
        return RealC39550Ga(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC39550Ga {
        return RealC39550Ga(value: val);
    }
}

let obj1 = RealC39550Ga.create_default();
let obj2 = RealC39550Ga.create_custom(79);

print(obj1.get_value());
print(obj2.get_value());
