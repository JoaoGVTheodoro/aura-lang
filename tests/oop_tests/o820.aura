// OOP: Mixin-like Composition
class AbstractC75238Cy {
    public def log(msg: str) { print(msg); }
}

class ConcreteC97547Au {
    private let logger: AbstractC75238Cy
    public let value: int
    
    def init(val: int, logger: AbstractC75238Cy) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = AbstractC75238Cy();
let obj = ConcreteC97547Au(val: 50, logger: logger);
print(obj.process());
