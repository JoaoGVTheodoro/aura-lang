// OOP: Static Members
class ConcreteC30347Te {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC30347Te.instance_count = ConcreteC30347Te.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC30347Te.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC30347Te(id: 1);
let obj2 = ConcreteC30347Te(id: 2);
let obj3 = ConcreteC30347Te(id: 3);

print(ConcreteC30347Te.instance_count);
print(obj1.get_id());
print(obj3.get_id());
