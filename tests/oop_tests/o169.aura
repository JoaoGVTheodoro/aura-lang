// OOP: Deep Inheritance Chain (4 levels)
class ConcreteC17914Fq {
    protected let base_val: int = 5
    public def get_val() -> int { return self.base_val; }
}

class ConcreteC27179Ib extends ConcreteC17914Fq {
    public def get_val() -> int { return self.base_val * 2; }
}

class BaseC18457Lg extends ConcreteC27179Ib {
    public def get_val() -> int { return self.base_val * 3; }
}

class C35322Vn extends BaseC18457Lg {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = C35322Vn(base_val: 5);
print(obj.get_val());
