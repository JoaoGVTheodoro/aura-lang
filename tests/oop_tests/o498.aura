// OOP: Mixin-like Composition
class ImplC98019Pk {
    public def log(msg: str) { print(msg); }
}

class BaseC25037Co {
    private let logger: ImplC98019Pk
    public let value: int
    
    def init(val: int, logger: ImplC98019Pk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC98019Pk();
let obj = BaseC25037Co(val: 50, logger: logger);
print(obj.process());
