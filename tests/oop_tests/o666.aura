// OOP: Mixin-like Composition
class C65986Dk {
    public def log(msg: str) { print(msg); }
}

class BaseC12470Mj {
    private let logger: C65986Dk
    public let value: int
    
    def init(val: int, logger: C65986Dk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C65986Dk();
let obj = BaseC12470Mj(val: 50, logger: logger);
print(obj.process());
