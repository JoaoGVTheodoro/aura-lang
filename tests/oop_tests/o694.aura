// OOP: Mixin-like Composition
class RealC81149Bf {
    public def log(msg: str) { print(msg); }
}

class C63007Xs {
    private let logger: RealC81149Bf
    public let value: int
    
    def init(val: int, logger: RealC81149Bf) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC81149Bf();
let obj = C63007Xs(val: 50, logger: logger);
print(obj.process());
