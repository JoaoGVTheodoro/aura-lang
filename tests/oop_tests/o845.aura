// OOP: Static Factory Pattern
class AbstractC21543Ng {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC21543Ng {
        return AbstractC21543Ng(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC21543Ng {
        return AbstractC21543Ng(value: val);
    }
}

let obj1 = AbstractC21543Ng.create_default();
let obj2 = AbstractC21543Ng.create_custom(13);

print(obj1.get_value());
print(obj2.get_value());
