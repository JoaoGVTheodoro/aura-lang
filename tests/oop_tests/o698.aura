// OOP: Delegation
class MockC20230Ea {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ConcreteC45178Yy {
    private let delegate: MockC20230Ea
    
    def init(t: MockC20230Ea) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = MockC20230Ea();
target_obj.data = 25;
let delegator_obj = ConcreteC45178Yy(t: target_obj);
print(delegator_obj.process());
