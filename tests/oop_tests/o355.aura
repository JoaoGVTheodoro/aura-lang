// OOP: Static Factory Pattern
class AbstractC23984Zi {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC23984Zi {
        return AbstractC23984Zi(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC23984Zi {
        return AbstractC23984Zi(value: val);
    }
}

let obj1 = AbstractC23984Zi.create_default();
let obj2 = AbstractC23984Zi.create_custom(24);

print(obj1.get_value());
print(obj2.get_value());
