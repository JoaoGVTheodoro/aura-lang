// OOP: Composition
class MockC23596Lm {
    public let data: int = 36
    public def process() -> int { return self.data * 2; }
}

class MockC47126Kx {
    private let component: MockC23596Lm
    
    def init(comp: MockC23596Lm) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC23596Lm(data: 36);
let outer_obj = MockC47126Kx(comp: inner_obj);
print(outer_obj.transform());
