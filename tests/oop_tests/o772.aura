// OOP: Static Members
class ImplC14687Rt {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ImplC14687Rt.instance_count = ImplC14687Rt.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ImplC14687Rt.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ImplC14687Rt(id: 1);
let obj2 = ImplC14687Rt(id: 2);
let obj3 = ImplC14687Rt(id: 3);

print(ImplC14687Rt.instance_count);
print(obj1.get_id());
print(obj3.get_id());
