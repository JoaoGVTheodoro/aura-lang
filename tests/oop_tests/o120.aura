// OOP: Mixin-like Composition
class BaseC19527Og {
    public def log(msg: str) { print(msg); }
}

class ConcreteC11084Wl {
    private let logger: BaseC19527Og
    public let value: int
    
    def init(val: int, logger: BaseC19527Og) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC19527Og();
let obj = ConcreteC11084Wl(val: 50, logger: logger);
print(obj.process());
