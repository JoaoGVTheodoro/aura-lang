// OOP: Mixin-like Composition
class BaseC84531Js {
    public def log(msg: str) { print(msg); }
}

class C22402Se {
    private let logger: BaseC84531Js
    public let value: int
    
    def init(val: int, logger: BaseC84531Js) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC84531Js();
let obj = C22402Se(val: 50, logger: logger);
print(obj.process());
