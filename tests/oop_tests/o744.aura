// OOP: Static Members
class C47419Dp {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C47419Dp.instance_count = C47419Dp.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C47419Dp.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C47419Dp(id: 1);
let obj2 = C47419Dp(id: 2);
let obj3 = C47419Dp(id: 3);

print(C47419Dp.instance_count);
print(obj1.get_id());
print(obj3.get_id());
