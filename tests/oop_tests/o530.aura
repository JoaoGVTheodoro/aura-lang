// OOP: Delegation
class C32527Wh {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC77800Am {
    private let delegate: C32527Wh
    
    def init(t: C32527Wh) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = C32527Wh();
target_obj.data = 25;
let delegator_obj = BaseC77800Am(t: target_obj);
print(delegator_obj.process());
