// OOP: Static Members
class ConcreteC67811Il {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC67811Il.instance_count = ConcreteC67811Il.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC67811Il.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC67811Il(id: 1);
let obj2 = ConcreteC67811Il(id: 2);
let obj3 = ConcreteC67811Il(id: 3);

print(ConcreteC67811Il.instance_count);
print(obj1.get_id());
print(obj3.get_id());
