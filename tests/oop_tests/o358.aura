// OOP: Mixin-like Composition
class ConcreteC26375Fy {
    public def log(msg: str) { print(msg); }
}

class C72881Pn {
    private let logger: ConcreteC26375Fy
    public let value: int
    
    def init(val: int, logger: ConcreteC26375Fy) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC26375Fy();
let obj = C72881Pn(val: 50, logger: logger);
print(obj.process());
