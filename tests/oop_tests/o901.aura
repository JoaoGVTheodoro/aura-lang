// OOP: Static Factory Pattern
class C19572Bp {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C19572Bp {
        return C19572Bp(value: 42);
    }
    
    public static def create_custom(val: int) -> C19572Bp {
        return C19572Bp(value: val);
    }
}

let obj1 = C19572Bp.create_default();
let obj2 = C19572Bp.create_custom(62);

print(obj1.get_value());
print(obj2.get_value());
