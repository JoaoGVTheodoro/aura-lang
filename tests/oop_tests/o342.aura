// OOP: Builder Pattern
class BaseC36026ZcBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> BaseC36026ZcBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> BaseC36026ZcBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> BaseC36026Zc {
        return BaseC36026Zc(name: self.name, age: self.age, email: self.email);
    }
}

class BaseC36026Zc {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = BaseC36026ZcBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
