// OOP: Delegation
class C55785Ba {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ImplC12673Yr {
    private let delegate: C55785Ba
    
    def init(t: C55785Ba) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = C55785Ba();
target_obj.data = 25;
let delegator_obj = ImplC12673Yr(t: target_obj);
print(delegator_obj.process());
