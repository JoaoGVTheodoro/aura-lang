// OOP: Static Members
class ConcreteC83382Af {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC83382Af.instance_count = ConcreteC83382Af.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC83382Af.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC83382Af(id: 1);
let obj2 = ConcreteC83382Af(id: 2);
let obj3 = ConcreteC83382Af(id: 3);

print(ConcreteC83382Af.instance_count);
print(obj1.get_id());
print(obj3.get_id());
