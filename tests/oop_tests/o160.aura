// OOP: Builder Pattern
class ImplC56598DxBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ImplC56598DxBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ImplC56598DxBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ImplC56598Dx {
        return ImplC56598Dx(name: self.name, age: self.age, email: self.email);
    }
}

class ImplC56598Dx {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ImplC56598DxBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
