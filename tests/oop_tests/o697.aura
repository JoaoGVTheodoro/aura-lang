// OOP: Composition
class C60717Mp {
    public let data: int = 75
    public def process() -> int { return self.data * 2; }
}

class AbstractC16484Xz {
    private let component: C60717Mp
    
    def init(comp: C60717Mp) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C60717Mp(data: 75);
let outer_obj = AbstractC16484Xz(comp: inner_obj);
print(outer_obj.transform());
