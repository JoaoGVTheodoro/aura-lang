// OOP: Composition
class BaseC83529Jt {
    public let data: int = 2
    public def process() -> int { return self.data * 2; }
}

class C54909Ov {
    private let component: BaseC83529Jt
    
    def init(comp: BaseC83529Jt) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC83529Jt(data: 2);
let outer_obj = C54909Ov(comp: inner_obj);
print(outer_obj.transform());
