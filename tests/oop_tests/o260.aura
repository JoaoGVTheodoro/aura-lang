// OOP: Mixin-like Composition
class RealC26431Ia {
    public def log(msg: str) { print(msg); }
}

class MockC42602Wz {
    private let logger: RealC26431Ia
    public let value: int
    
    def init(val: int, logger: RealC26431Ia) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC26431Ia();
let obj = MockC42602Wz(val: 50, logger: logger);
print(obj.process());
