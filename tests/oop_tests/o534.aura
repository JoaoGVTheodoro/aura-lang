// OOP: Static Members
class BaseC97955Rl {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC97955Rl.instance_count = BaseC97955Rl.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC97955Rl.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC97955Rl(id: 1);
let obj2 = BaseC97955Rl(id: 2);
let obj3 = BaseC97955Rl(id: 3);

print(BaseC97955Rl.instance_count);
print(obj1.get_id());
print(obj3.get_id());
