// OOP: Static Factory Pattern
class ImplC26707Gq {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC26707Gq {
        return ImplC26707Gq(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC26707Gq {
        return ImplC26707Gq(value: val);
    }
}

let obj1 = ImplC26707Gq.create_default();
let obj2 = ImplC26707Gq.create_custom(68);

print(obj1.get_value());
print(obj2.get_value());
