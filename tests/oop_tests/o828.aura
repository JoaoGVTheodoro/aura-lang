// OOP: Static Members
class BaseC76489Ww {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC76489Ww.instance_count = BaseC76489Ww.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC76489Ww.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC76489Ww(id: 1);
let obj2 = BaseC76489Ww(id: 2);
let obj3 = BaseC76489Ww(id: 3);

print(BaseC76489Ww.instance_count);
print(obj1.get_id());
print(obj3.get_id());
