// OOP: Delegation
class RealC15737Cz {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC31452Dj {
    private let delegate: RealC15737Cz
    
    def init(t: RealC15737Cz) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = RealC15737Cz();
target_obj.data = 25;
let delegator_obj = BaseC31452Dj(t: target_obj);
print(delegator_obj.process());
