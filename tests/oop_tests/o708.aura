// OOP: Mixin-like Composition
class ConcreteC31332Yc {
    public def log(msg: str) { print(msg); }
}

class ImplC27812Yx {
    private let logger: ConcreteC31332Yc
    public let value: int
    
    def init(val: int, logger: ConcreteC31332Yc) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC31332Yc();
let obj = ImplC27812Yx(val: 50, logger: logger);
print(obj.process());
