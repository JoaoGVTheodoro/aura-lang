// OOP: Static Factory Pattern
class C33217Bt {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C33217Bt {
        return C33217Bt(value: 42);
    }
    
    public static def create_custom(val: int) -> C33217Bt {
        return C33217Bt(value: val);
    }
}

let obj1 = C33217Bt.create_default();
let obj2 = C33217Bt.create_custom(72);

print(obj1.get_value());
print(obj2.get_value());
