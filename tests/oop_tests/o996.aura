// OOP: Static Members
class BaseC68966Gg {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC68966Gg.instance_count = BaseC68966Gg.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC68966Gg.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC68966Gg(id: 1);
let obj2 = BaseC68966Gg(id: 2);
let obj3 = BaseC68966Gg(id: 3);

print(BaseC68966Gg.instance_count);
print(obj1.get_id());
print(obj3.get_id());
