// OOP: Deep Inheritance Chain (4 levels)
class ConcreteC36776Cw {
    protected let base_val: int = 13
    public def get_val() -> int { return self.base_val; }
}

class AbstractC87508Pu extends ConcreteC36776Cw {
    public def get_val() -> int { return self.base_val * 2; }
}

class RealC65947Rp extends AbstractC87508Pu {
    public def get_val() -> int { return self.base_val * 3; }
}

class C48426Hm extends RealC65947Rp {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = C48426Hm(base_val: 13);
print(obj.get_val());
