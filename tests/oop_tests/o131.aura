// OOP: Static Factory Pattern
class ImplC24375Mc {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC24375Mc {
        return ImplC24375Mc(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC24375Mc {
        return ImplC24375Mc(value: val);
    }
}

let obj1 = ImplC24375Mc.create_default();
let obj2 = ImplC24375Mc.create_custom(67);

print(obj1.get_value());
print(obj2.get_value());
