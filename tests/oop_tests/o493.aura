// OOP: Method Variants
class RealC63977Xo {
    public let x: int
    public let y: int
    
    def init(x: int, y: int) {
        self.x = x;
        self.y = y;
    }
    
    // Expression method
    public def add() -> int = self.x + self.y
    
    // Block method
    public def multiply() -> int {
        return self.x * self.y;
    }
    
    // Computed method
    public def sum_squares() -> int {
        let sum = self.x * self.x + self.y * self.y;
        return sum;
    }
}

let obj = RealC63977Xo(x: 44, y: 35);
print(obj.add());
print(obj.multiply());
print(obj.sum_squares());
