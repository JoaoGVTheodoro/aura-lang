// OOP: Mixin-like Composition
class ConcreteC97203Co {
    public def log(msg: str) { print(msg); }
}

class ImplC19314Yz {
    private let logger: ConcreteC97203Co
    public let value: int
    
    def init(val: int, logger: ConcreteC97203Co) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC97203Co();
let obj = ImplC19314Yz(val: 50, logger: logger);
print(obj.process());
