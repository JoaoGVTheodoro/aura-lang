// OOP: Static Factory Pattern
class MockC26410In {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC26410In {
        return MockC26410In(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC26410In {
        return MockC26410In(value: val);
    }
}

let obj1 = MockC26410In.create_default();
let obj2 = MockC26410In.create_custom(9);

print(obj1.get_value());
print(obj2.get_value());
