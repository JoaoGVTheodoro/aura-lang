// OOP: Delegation
class BaseC55162Gr {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC44484Ja {
    private let delegate: BaseC55162Gr
    
    def init(t: BaseC55162Gr) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC55162Gr();
target_obj.data = 25;
let delegator_obj = BaseC44484Ja(t: target_obj);
print(delegator_obj.process());
