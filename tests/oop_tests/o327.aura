// OOP: Static Factory Pattern
class ImplC56807Gm {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC56807Gm {
        return ImplC56807Gm(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC56807Gm {
        return ImplC56807Gm(value: val);
    }
}

let obj1 = ImplC56807Gm.create_default();
let obj2 = ImplC56807Gm.create_custom(43);

print(obj1.get_value());
print(obj2.get_value());
