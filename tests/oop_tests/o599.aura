// OOP: Composition
class ConcreteC78953Cb {
    public let data: int = 38
    public def process() -> int { return self.data * 2; }
}

class C15564Oy {
    private let component: ConcreteC78953Cb
    
    def init(comp: ConcreteC78953Cb) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ConcreteC78953Cb(data: 38);
let outer_obj = C15564Oy(comp: inner_obj);
print(outer_obj.transform());
