// OOP: Composition
class RealC63756Qo {
    public let data: int = 42
    public def process() -> int { return self.data * 2; }
}

class MockC22772Bs {
    private let component: RealC63756Qo
    
    def init(comp: RealC63756Qo) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC63756Qo(data: 42);
let outer_obj = MockC22772Bs(comp: inner_obj);
print(outer_obj.transform());
