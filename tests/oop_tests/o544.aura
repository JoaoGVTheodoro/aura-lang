// OOP: Delegation
class RealC82087Rw {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class AbstractC92468Vh {
    private let delegate: RealC82087Rw
    
    def init(t: RealC82087Rw) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = RealC82087Rw();
target_obj.data = 25;
let delegator_obj = AbstractC92468Vh(t: target_obj);
print(delegator_obj.process());
