// OOP: Composition
class RealC30129Ho {
    public let data: int = 91
    public def process() -> int { return self.data * 2; }
}

class RealC49922At {
    private let component: RealC30129Ho
    
    def init(comp: RealC30129Ho) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC30129Ho(data: 91);
let outer_obj = RealC49922At(comp: inner_obj);
print(outer_obj.transform());
