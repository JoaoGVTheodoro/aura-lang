// OOP: Mixin-like Composition
class RealC17878Mr {
    public def log(msg: str) { print(msg); }
}

class RealC30380Ik {
    private let logger: RealC17878Mr
    public let value: int
    
    def init(val: int, logger: RealC17878Mr) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC17878Mr();
let obj = RealC30380Ik(val: 50, logger: logger);
print(obj.process());
