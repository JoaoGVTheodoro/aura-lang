// OOP: Static Members
class RealC68901Ca {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC68901Ca.instance_count = RealC68901Ca.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC68901Ca.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC68901Ca(id: 1);
let obj2 = RealC68901Ca(id: 2);
let obj3 = RealC68901Ca(id: 3);

print(RealC68901Ca.instance_count);
print(obj1.get_id());
print(obj3.get_id());
