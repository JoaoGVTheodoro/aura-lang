// OOP: Static Factory Pattern
class ImplC24068Lu {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC24068Lu {
        return ImplC24068Lu(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC24068Lu {
        return ImplC24068Lu(value: val);
    }
}

let obj1 = ImplC24068Lu.create_default();
let obj2 = ImplC24068Lu.create_custom(33);

print(obj1.get_value());
print(obj2.get_value());
