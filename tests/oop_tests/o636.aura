// OOP: Builder Pattern
class ConcreteC77588IlBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ConcreteC77588IlBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ConcreteC77588IlBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ConcreteC77588Il {
        return ConcreteC77588Il(name: self.name, age: self.age, email: self.email);
    }
}

class ConcreteC77588Il {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ConcreteC77588IlBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
