// OOP: Static Members
class AbstractC41168Qi {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC41168Qi.instance_count = AbstractC41168Qi.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC41168Qi.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC41168Qi(id: 1);
let obj2 = AbstractC41168Qi(id: 2);
let obj3 = AbstractC41168Qi(id: 3);

print(AbstractC41168Qi.instance_count);
print(obj1.get_id());
print(obj3.get_id());
