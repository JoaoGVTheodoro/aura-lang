// OOP: Mixin-like Composition
class C87170Wq {
    public def log(msg: str) { print(msg); }
}

class RealC18152Tv {
    private let logger: C87170Wq
    public let value: int
    
    def init(val: int, logger: C87170Wq) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C87170Wq();
let obj = RealC18152Tv(val: 50, logger: logger);
print(obj.process());
