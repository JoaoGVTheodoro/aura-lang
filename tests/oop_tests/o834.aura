// OOP: Mixin-like Composition
class RealC45641Ls {
    public def log(msg: str) { print(msg); }
}

class BaseC95022Se {
    private let logger: RealC45641Ls
    public let value: int
    
    def init(val: int, logger: RealC45641Ls) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC45641Ls();
let obj = BaseC95022Se(val: 50, logger: logger);
print(obj.process());
