// OOP: Static Members
class C31184Zy {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C31184Zy.instance_count = C31184Zy.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C31184Zy.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C31184Zy(id: 1);
let obj2 = C31184Zy(id: 2);
let obj3 = C31184Zy(id: 3);

print(C31184Zy.instance_count);
print(obj1.get_id());
print(obj3.get_id());
