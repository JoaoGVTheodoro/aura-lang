// OOP: Mixin-like Composition
class ImplC87759Ul {
    public def log(msg: str) { print(msg); }
}

class ConcreteC65508Xy {
    private let logger: ImplC87759Ul
    public let value: int
    
    def init(val: int, logger: ImplC87759Ul) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC87759Ul();
let obj = ConcreteC65508Xy(val: 50, logger: logger);
print(obj.process());
