// OOP: Mixin-like Composition
class RealC75186Bi {
    public def log(msg: str) { print(msg); }
}

class BaseC60654Xd {
    private let logger: RealC75186Bi
    public let value: int
    
    def init(val: int, logger: RealC75186Bi) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC75186Bi();
let obj = BaseC60654Xd(val: 50, logger: logger);
print(obj.process());
