// OOP: Static Members
class AbstractC61782Da {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC61782Da.instance_count = AbstractC61782Da.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC61782Da.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC61782Da(id: 1);
let obj2 = AbstractC61782Da(id: 2);
let obj3 = AbstractC61782Da(id: 3);

print(AbstractC61782Da.instance_count);
print(obj1.get_id());
print(obj3.get_id());
