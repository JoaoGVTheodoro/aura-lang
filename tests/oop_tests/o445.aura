// OOP: Composition
class BaseC89827Rc {
    public let data: int = 52
    public def process() -> int { return self.data * 2; }
}

class ConcreteC36187Ik {
    private let component: BaseC89827Rc
    
    def init(comp: BaseC89827Rc) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC89827Rc(data: 52);
let outer_obj = ConcreteC36187Ik(comp: inner_obj);
print(outer_obj.transform());
