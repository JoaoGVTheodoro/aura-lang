// OOP: Mixin-like Composition
class C44779Uu {
    public def log(msg: str) { print(msg); }
}

class RealC40822Kx {
    private let logger: C44779Uu
    public let value: int
    
    def init(val: int, logger: C44779Uu) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C44779Uu();
let obj = RealC40822Kx(val: 50, logger: logger);
print(obj.process());
