// OOP: Method Variants
class ConcreteC45244Xi {
    public let x: int
    public let y: int
    
    def init(x: int, y: int) {
        self.x = x;
        self.y = y;
    }
    
    // Expression method
    public def add() -> int = self.x + self.y
    
    // Block method
    public def multiply() -> int {
        return self.x * self.y;
    }
    
    // Computed method
    public def sum_squares() -> int {
        let sum = self.x * self.x + self.y * self.y;
        return sum;
    }
}

let obj = ConcreteC45244Xi(x: 11, y: 27);
print(obj.add());
print(obj.multiply());
print(obj.sum_squares());
