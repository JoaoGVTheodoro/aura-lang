// OOP: Mixin-like Composition
class BaseC21547Hl {
    public def log(msg: str) { print(msg); }
}

class ImplC20474Jv {
    private let logger: BaseC21547Hl
    public let value: int
    
    def init(val: int, logger: BaseC21547Hl) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC21547Hl();
let obj = ImplC20474Jv(val: 50, logger: logger);
print(obj.process());
