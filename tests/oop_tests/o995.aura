// OOP: Deep Inheritance Chain (3 levels)
class RealC23287Lp {
    protected let base_val: int = 2
    public def get_val() -> int { return self.base_val; }
}

class BaseC94896Zl extends RealC23287Lp {
    public def get_val() -> int { return self.base_val * 2; }
}

class BaseC66521Wm extends BaseC94896Zl {
    public def get_val() -> int { return self.base_val * 3; }
}

let obj = BaseC66521Wm(base_val: 2);
print(obj.get_val());
