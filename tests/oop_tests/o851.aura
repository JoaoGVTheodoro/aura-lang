// OOP: Composition
class BaseC36540Rc {
    public let data: int = 5
    public def process() -> int { return self.data * 2; }
}

class BaseC32753Qo {
    private let component: BaseC36540Rc
    
    def init(comp: BaseC36540Rc) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC36540Rc(data: 5);
let outer_obj = BaseC32753Qo(comp: inner_obj);
print(outer_obj.transform());
