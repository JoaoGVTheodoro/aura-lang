// OOP: Delegation
class ConcreteC49076Pt {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class C35426Du {
    private let delegate: ConcreteC49076Pt
    
    def init(t: ConcreteC49076Pt) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC49076Pt();
target_obj.data = 25;
let delegator_obj = C35426Du(t: target_obj);
print(delegator_obj.process());
