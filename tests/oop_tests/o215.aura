// OOP: Static Factory Pattern
class AbstractC83666Bs {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC83666Bs {
        return AbstractC83666Bs(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC83666Bs {
        return AbstractC83666Bs(value: val);
    }
}

let obj1 = AbstractC83666Bs.create_default();
let obj2 = AbstractC83666Bs.create_custom(35);

print(obj1.get_value());
print(obj2.get_value());
