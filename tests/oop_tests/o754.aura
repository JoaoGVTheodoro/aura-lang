// OOP: Delegation
class BaseC19295Im {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC58794Xp {
    private let delegate: BaseC19295Im
    
    def init(t: BaseC19295Im) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC19295Im();
target_obj.data = 25;
let delegator_obj = BaseC58794Xp(t: target_obj);
print(delegator_obj.process());
