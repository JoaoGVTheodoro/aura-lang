// OOP: Static Members
class C63974Kz {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C63974Kz.instance_count = C63974Kz.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C63974Kz.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C63974Kz(id: 1);
let obj2 = C63974Kz(id: 2);
let obj3 = C63974Kz(id: 3);

print(C63974Kz.instance_count);
print(obj1.get_id());
print(obj3.get_id());
