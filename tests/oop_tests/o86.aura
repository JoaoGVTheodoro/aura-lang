// OOP: Static Members
class C52162Te {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C52162Te.instance_count = C52162Te.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C52162Te.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C52162Te(id: 1);
let obj2 = C52162Te(id: 2);
let obj3 = C52162Te(id: 3);

print(C52162Te.instance_count);
print(obj1.get_id());
print(obj3.get_id());
