// OOP: Static Factory Pattern
class RealC41235Be {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC41235Be {
        return RealC41235Be(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC41235Be {
        return RealC41235Be(value: val);
    }
}

let obj1 = RealC41235Be.create_default();
let obj2 = RealC41235Be.create_custom(37);

print(obj1.get_value());
print(obj2.get_value());
