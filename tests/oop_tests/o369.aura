// OOP: Static Factory Pattern
class C14523Cf {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C14523Cf {
        return C14523Cf(value: 42);
    }
    
    public static def create_custom(val: int) -> C14523Cf {
        return C14523Cf(value: val);
    }
}

let obj1 = C14523Cf.create_default();
let obj2 = C14523Cf.create_custom(43);

print(obj1.get_value());
print(obj2.get_value());
