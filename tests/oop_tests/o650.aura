// OOP: Builder Pattern
class BaseC27992RcBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> BaseC27992RcBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> BaseC27992RcBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> BaseC27992Rc {
        return BaseC27992Rc(name: self.name, age: self.age, email: self.email);
    }
}

class BaseC27992Rc {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = BaseC27992RcBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
