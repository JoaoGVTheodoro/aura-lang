// OOP: Static Factory Pattern
class BaseC69880En {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC69880En {
        return BaseC69880En(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC69880En {
        return BaseC69880En(value: val);
    }
}

let obj1 = BaseC69880En.create_default();
let obj2 = BaseC69880En.create_custom(73);

print(obj1.get_value());
print(obj2.get_value());
