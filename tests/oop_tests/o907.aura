// OOP: Composition
class MockC48343Zu {
    public let data: int = 14
    public def process() -> int { return self.data * 2; }
}

class MockC18814Mh {
    private let component: MockC48343Zu
    
    def init(comp: MockC48343Zu) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC48343Zu(data: 14);
let outer_obj = MockC18814Mh(comp: inner_obj);
print(outer_obj.transform());
