// OOP: Static Members
class AbstractC84819Ct {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC84819Ct.instance_count = AbstractC84819Ct.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC84819Ct.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC84819Ct(id: 1);
let obj2 = AbstractC84819Ct(id: 2);
let obj3 = AbstractC84819Ct(id: 3);

print(AbstractC84819Ct.instance_count);
print(obj1.get_id());
print(obj3.get_id());
