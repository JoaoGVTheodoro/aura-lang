// OOP: Static Members
class C67104Nx {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C67104Nx.instance_count = C67104Nx.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C67104Nx.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C67104Nx(id: 1);
let obj2 = C67104Nx(id: 2);
let obj3 = C67104Nx(id: 3);

print(C67104Nx.instance_count);
print(obj1.get_id());
print(obj3.get_id());
