// OOP: Composition
class BaseC89199Qj {
    public let data: int = 27
    public def process() -> int { return self.data * 2; }
}

class ConcreteC26867Jh {
    private let component: BaseC89199Qj
    
    def init(comp: BaseC89199Qj) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC89199Qj(data: 27);
let outer_obj = ConcreteC26867Jh(comp: inner_obj);
print(outer_obj.transform());
