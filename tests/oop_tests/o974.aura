// OOP: Mixin-like Composition
class ConcreteC55216Mx {
    public def log(msg: str) { print(msg); }
}

class RealC29445Zb {
    private let logger: ConcreteC55216Mx
    public let value: int
    
    def init(val: int, logger: ConcreteC55216Mx) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC55216Mx();
let obj = RealC29445Zb(val: 50, logger: logger);
print(obj.process());
