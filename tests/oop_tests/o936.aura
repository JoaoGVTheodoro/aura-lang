// OOP: Delegation
class ConcreteC35731Fo {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class AbstractC53407Pn {
    private let delegate: ConcreteC35731Fo
    
    def init(t: ConcreteC35731Fo) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC35731Fo();
target_obj.data = 25;
let delegator_obj = AbstractC53407Pn(t: target_obj);
print(delegator_obj.process());
