// OOP: Delegation
class C83596Tt {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC81649Bg {
    private let delegate: C83596Tt
    
    def init(t: C83596Tt) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = C83596Tt();
target_obj.data = 25;
let delegator_obj = BaseC81649Bg(t: target_obj);
print(delegator_obj.process());
