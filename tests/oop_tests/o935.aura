// OOP: Composition
class C90374Uo {
    public let data: int = 8
    public def process() -> int { return self.data * 2; }
}

class C98683Wf {
    private let component: C90374Uo
    
    def init(comp: C90374Uo) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C90374Uo(data: 8);
let outer_obj = C98683Wf(comp: inner_obj);
print(outer_obj.transform());
