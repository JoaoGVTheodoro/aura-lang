// OOP: Composition
class C54417Dw {
    public let data: int = 88
    public def process() -> int { return self.data * 2; }
}

class MockC57420Zy {
    private let component: C54417Dw
    
    def init(comp: C54417Dw) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C54417Dw(data: 88);
let outer_obj = MockC57420Zy(comp: inner_obj);
print(outer_obj.transform());
