// OOP: Static Factory Pattern
class AbstractC17344Jd {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC17344Jd {
        return AbstractC17344Jd(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC17344Jd {
        return AbstractC17344Jd(value: val);
    }
}

let obj1 = AbstractC17344Jd.create_default();
let obj2 = AbstractC17344Jd.create_custom(53);

print(obj1.get_value());
print(obj2.get_value());
