// OOP: Deep Inheritance Chain (5 levels)
class ConcreteC88442Dw {
    protected let base_val: int = 36
    public def get_val() -> int { return self.base_val; }
}

class BaseC15276Kd extends ConcreteC88442Dw {
    public def get_val() -> int { return self.base_val * 2; }
}

class BaseC12290Hh extends BaseC15276Kd {
    public def get_val() -> int { return self.base_val * 3; }
}

class BaseC26714Rb extends BaseC12290Hh {
    public def get_val() -> int { return self.base_val * 4; }
}

class ImplC16190Ys extends BaseC26714Rb {
    public def get_val() -> int { return self.base_val * 5; }
}

let obj = ImplC16190Ys(base_val: 36);
print(obj.get_val());
