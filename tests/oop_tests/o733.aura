// OOP: Static Factory Pattern
class AbstractC57188Kn {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC57188Kn {
        return AbstractC57188Kn(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC57188Kn {
        return AbstractC57188Kn(value: val);
    }
}

let obj1 = AbstractC57188Kn.create_default();
let obj2 = AbstractC57188Kn.create_custom(72);

print(obj1.get_value());
print(obj2.get_value());
