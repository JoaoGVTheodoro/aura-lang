// OOP: Static Members
class ConcreteC48442Dh {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC48442Dh.instance_count = ConcreteC48442Dh.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC48442Dh.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC48442Dh(id: 1);
let obj2 = ConcreteC48442Dh(id: 2);
let obj3 = ConcreteC48442Dh(id: 3);

print(ConcreteC48442Dh.instance_count);
print(obj1.get_id());
print(obj3.get_id());
