// OOP: Static Factory Pattern
class ConcreteC72588Qx {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ConcreteC72588Qx {
        return ConcreteC72588Qx(value: 42);
    }
    
    public static def create_custom(val: int) -> ConcreteC72588Qx {
        return ConcreteC72588Qx(value: val);
    }
}

let obj1 = ConcreteC72588Qx.create_default();
let obj2 = ConcreteC72588Qx.create_custom(43);

print(obj1.get_value());
print(obj2.get_value());
