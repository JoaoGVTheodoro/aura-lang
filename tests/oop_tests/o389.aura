// OOP: Composition
class C18917Jt {
    public let data: int = 29
    public def process() -> int { return self.data * 2; }
}

class C59371Jy {
    private let component: C18917Jt
    
    def init(comp: C18917Jt) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C18917Jt(data: 29);
let outer_obj = C59371Jy(comp: inner_obj);
print(outer_obj.transform());
