// OOP: Static Members
class MockC11588Ao {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC11588Ao.instance_count = MockC11588Ao.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC11588Ao.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC11588Ao(id: 1);
let obj2 = MockC11588Ao(id: 2);
let obj3 = MockC11588Ao(id: 3);

print(MockC11588Ao.instance_count);
print(obj1.get_id());
print(obj3.get_id());
