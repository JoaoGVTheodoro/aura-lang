// OOP: Static Factory Pattern
class C12329Ju {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C12329Ju {
        return C12329Ju(value: 42);
    }
    
    public static def create_custom(val: int) -> C12329Ju {
        return C12329Ju(value: val);
    }
}

let obj1 = C12329Ju.create_default();
let obj2 = C12329Ju.create_custom(48);

print(obj1.get_value());
print(obj2.get_value());
