// OOP: Static Factory Pattern
class MockC95827Yz {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC95827Yz {
        return MockC95827Yz(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC95827Yz {
        return MockC95827Yz(value: val);
    }
}

let obj1 = MockC95827Yz.create_default();
let obj2 = MockC95827Yz.create_custom(22);

print(obj1.get_value());
print(obj2.get_value());
