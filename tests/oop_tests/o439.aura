// OOP: Static Factory Pattern
class MockC54228Dn {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC54228Dn {
        return MockC54228Dn(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC54228Dn {
        return MockC54228Dn(value: val);
    }
}

let obj1 = MockC54228Dn.create_default();
let obj2 = MockC54228Dn.create_custom(49);

print(obj1.get_value());
print(obj2.get_value());
