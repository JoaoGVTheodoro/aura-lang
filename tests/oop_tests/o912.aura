// OOP: Static Members
class ConcreteC87264Fm {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC87264Fm.instance_count = ConcreteC87264Fm.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC87264Fm.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC87264Fm(id: 1);
let obj2 = ConcreteC87264Fm(id: 2);
let obj3 = ConcreteC87264Fm(id: 3);

print(ConcreteC87264Fm.instance_count);
print(obj1.get_id());
print(obj3.get_id());
