// OOP: Static Members
class C46549Ub {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C46549Ub.instance_count = C46549Ub.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C46549Ub.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C46549Ub(id: 1);
let obj2 = C46549Ub(id: 2);
let obj3 = C46549Ub(id: 3);

print(C46549Ub.instance_count);
print(obj1.get_id());
print(obj3.get_id());
