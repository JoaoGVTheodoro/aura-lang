// OOP: Static Members
class C53979Vi {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C53979Vi.instance_count = C53979Vi.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C53979Vi.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C53979Vi(id: 1);
let obj2 = C53979Vi(id: 2);
let obj3 = C53979Vi(id: 3);

print(C53979Vi.instance_count);
print(obj1.get_id());
print(obj3.get_id());
