// OOP: Delegation
class AbstractC78497Hu {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC90118Pp {
    private let delegate: AbstractC78497Hu
    
    def init(t: AbstractC78497Hu) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = AbstractC78497Hu();
target_obj.data = 25;
let delegator_obj = BaseC90118Pp(t: target_obj);
print(delegator_obj.process());
