// OOP: Composition
class ConcreteC96040En {
    public let data: int = 47
    public def process() -> int { return self.data * 2; }
}

class ConcreteC33999Ed {
    private let component: ConcreteC96040En
    
    def init(comp: ConcreteC96040En) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ConcreteC96040En(data: 47);
let outer_obj = ConcreteC33999Ed(comp: inner_obj);
print(outer_obj.transform());
