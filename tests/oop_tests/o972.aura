// OOP: Builder Pattern
class AbstractC11570TvBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> AbstractC11570TvBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> AbstractC11570TvBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> AbstractC11570Tv {
        return AbstractC11570Tv(name: self.name, age: self.age, email: self.email);
    }
}

class AbstractC11570Tv {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = AbstractC11570TvBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
