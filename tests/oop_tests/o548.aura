// OOP: Static Members
class ConcreteC36719Sq {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC36719Sq.instance_count = ConcreteC36719Sq.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC36719Sq.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC36719Sq(id: 1);
let obj2 = ConcreteC36719Sq(id: 2);
let obj3 = ConcreteC36719Sq(id: 3);

print(ConcreteC36719Sq.instance_count);
print(obj1.get_id());
print(obj3.get_id());
