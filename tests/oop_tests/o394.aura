// OOP: Static Members
class BaseC54686Gp {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC54686Gp.instance_count = BaseC54686Gp.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC54686Gp.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC54686Gp(id: 1);
let obj2 = BaseC54686Gp(id: 2);
let obj3 = BaseC54686Gp(id: 3);

print(BaseC54686Gp.instance_count);
print(obj1.get_id());
print(obj3.get_id());
