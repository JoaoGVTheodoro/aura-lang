// OOP: Static Members
class C32173Kf {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C32173Kf.instance_count = C32173Kf.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C32173Kf.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C32173Kf(id: 1);
let obj2 = C32173Kf(id: 2);
let obj3 = C32173Kf(id: 3);

print(C32173Kf.instance_count);
print(obj1.get_id());
print(obj3.get_id());
