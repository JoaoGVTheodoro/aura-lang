// OOP: Deep Inheritance Chain (3 levels)
class MockC51257Bj {
    protected let base_val: int = 26
    public def get_val() -> int { return self.base_val; }
}

class MockC79500Md extends MockC51257Bj {
    public def get_val() -> int { return self.base_val * 2; }
}

class MockC96136Zq extends MockC79500Md {
    public def get_val() -> int { return self.base_val * 3; }
}

let obj = MockC96136Zq(base_val: 26);
print(obj.get_val());
