// OOP: Static Members
class ImplC47123Kq {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ImplC47123Kq.instance_count = ImplC47123Kq.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ImplC47123Kq.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ImplC47123Kq(id: 1);
let obj2 = ImplC47123Kq(id: 2);
let obj3 = ImplC47123Kq(id: 3);

print(ImplC47123Kq.instance_count);
print(obj1.get_id());
print(obj3.get_id());
