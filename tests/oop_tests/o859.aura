// OOP: Static Factory Pattern
class MockC60697Yc {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC60697Yc {
        return MockC60697Yc(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC60697Yc {
        return MockC60697Yc(value: val);
    }
}

let obj1 = MockC60697Yc.create_default();
let obj2 = MockC60697Yc.create_custom(27);

print(obj1.get_value());
print(obj2.get_value());
