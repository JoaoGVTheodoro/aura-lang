// OOP: Static Factory Pattern
class C16453Hf {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C16453Hf {
        return C16453Hf(value: 42);
    }
    
    public static def create_custom(val: int) -> C16453Hf {
        return C16453Hf(value: val);
    }
}

let obj1 = C16453Hf.create_default();
let obj2 = C16453Hf.create_custom(6);

print(obj1.get_value());
print(obj2.get_value());
