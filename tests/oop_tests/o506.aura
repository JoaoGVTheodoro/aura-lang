// OOP: Static Members
class AbstractC76988De {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC76988De.instance_count = AbstractC76988De.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC76988De.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC76988De(id: 1);
let obj2 = AbstractC76988De(id: 2);
let obj3 = AbstractC76988De(id: 3);

print(AbstractC76988De.instance_count);
print(obj1.get_id());
print(obj3.get_id());
