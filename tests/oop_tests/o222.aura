// OOP: Delegation
class ImplC48302Po {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC32407Dh {
    private let delegate: ImplC48302Po
    
    def init(t: ImplC48302Po) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ImplC48302Po();
target_obj.data = 25;
let delegator_obj = BaseC32407Dh(t: target_obj);
print(delegator_obj.process());
