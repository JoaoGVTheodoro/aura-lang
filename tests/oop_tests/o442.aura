// OOP: Mixin-like Composition
class C68228Ik {
    public def log(msg: str) { print(msg); }
}

class BaseC85059Iw {
    private let logger: C68228Ik
    public let value: int
    
    def init(val: int, logger: C68228Ik) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C68228Ik();
let obj = BaseC85059Iw(val: 50, logger: logger);
print(obj.process());
