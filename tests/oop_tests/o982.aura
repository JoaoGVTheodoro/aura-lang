// OOP: Static Members
class ImplC46517Xn {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ImplC46517Xn.instance_count = ImplC46517Xn.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ImplC46517Xn.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ImplC46517Xn(id: 1);
let obj2 = ImplC46517Xn(id: 2);
let obj3 = ImplC46517Xn(id: 3);

print(ImplC46517Xn.instance_count);
print(obj1.get_id());
print(obj3.get_id());
