// OOP: Builder Pattern
class RealC21154XnBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> RealC21154XnBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> RealC21154XnBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> RealC21154Xn {
        return RealC21154Xn(name: self.name, age: self.age, email: self.email);
    }
}

class RealC21154Xn {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = RealC21154XnBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
