// OOP: Composition
class ConcreteC58487Im {
    public let data: int = 44
    public def process() -> int { return self.data * 2; }
}

class C32897Tb {
    private let component: ConcreteC58487Im
    
    def init(comp: ConcreteC58487Im) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ConcreteC58487Im(data: 44);
let outer_obj = C32897Tb(comp: inner_obj);
print(outer_obj.transform());
