// OOP: Static Members
class ImplC22628Mp {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ImplC22628Mp.instance_count = ImplC22628Mp.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ImplC22628Mp.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ImplC22628Mp(id: 1);
let obj2 = ImplC22628Mp(id: 2);
let obj3 = ImplC22628Mp(id: 3);

print(ImplC22628Mp.instance_count);
print(obj1.get_id());
print(obj3.get_id());
