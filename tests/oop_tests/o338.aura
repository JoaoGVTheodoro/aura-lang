// OOP: Static Members
class BaseC74590Xr {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC74590Xr.instance_count = BaseC74590Xr.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC74590Xr.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC74590Xr(id: 1);
let obj2 = BaseC74590Xr(id: 2);
let obj3 = BaseC74590Xr(id: 3);

print(BaseC74590Xr.instance_count);
print(obj1.get_id());
print(obj3.get_id());
