// OOP: Mixin-like Composition
class AbstractC99813Lx {
    public def log(msg: str) { print(msg); }
}

class C52164Zx {
    private let logger: AbstractC99813Lx
    public let value: int
    
    def init(val: int, logger: AbstractC99813Lx) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = AbstractC99813Lx();
let obj = C52164Zx(val: 50, logger: logger);
print(obj.process());
