// OOP: Mixin-like Composition
class ConcreteC93840Vk {
    public def log(msg: str) { print(msg); }
}

class BaseC53804Ux {
    private let logger: ConcreteC93840Vk
    public let value: int
    
    def init(val: int, logger: ConcreteC93840Vk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC93840Vk();
let obj = BaseC53804Ux(val: 50, logger: logger);
print(obj.process());
