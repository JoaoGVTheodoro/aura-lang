// OOP: Static Factory Pattern
class AbstractC95930Ow {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC95930Ow {
        return AbstractC95930Ow(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC95930Ow {
        return AbstractC95930Ow(value: val);
    }
}

let obj1 = AbstractC95930Ow.create_default();
let obj2 = AbstractC95930Ow.create_custom(35);

print(obj1.get_value());
print(obj2.get_value());
