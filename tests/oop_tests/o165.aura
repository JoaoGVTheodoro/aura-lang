// OOP: Composition
class MockC28405Mr {
    public let data: int = 4
    public def process() -> int { return self.data * 2; }
}

class MockC85711Zi {
    private let component: MockC28405Mr
    
    def init(comp: MockC28405Mr) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC28405Mr(data: 4);
let outer_obj = MockC85711Zi(comp: inner_obj);
print(outer_obj.transform());
