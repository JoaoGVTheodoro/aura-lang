// OOP: Static Members
class MockC91352Ep {
    public static let instance_count: int = 0
    public static def increment_count() -> void { MockC91352Ep.instance_count = MockC91352Ep.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        MockC91352Ep.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = MockC91352Ep(id: 1);
let obj2 = MockC91352Ep(id: 2);
let obj3 = MockC91352Ep(id: 3);

print(MockC91352Ep.instance_count);
print(obj1.get_id());
print(obj3.get_id());
