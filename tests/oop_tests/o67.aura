// OOP: Composition
class RealC41118Vs {
    public let data: int = 73
    public def process() -> int { return self.data * 2; }
}

class RealC49663Zv {
    private let component: RealC41118Vs
    
    def init(comp: RealC41118Vs) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC41118Vs(data: 73);
let outer_obj = RealC49663Zv(comp: inner_obj);
print(outer_obj.transform());
