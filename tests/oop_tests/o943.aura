// OOP: Static Factory Pattern
class AbstractC54183Or {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC54183Or {
        return AbstractC54183Or(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC54183Or {
        return AbstractC54183Or(value: val);
    }
}

let obj1 = AbstractC54183Or.create_default();
let obj2 = AbstractC54183Or.create_custom(82);

print(obj1.get_value());
print(obj2.get_value());
