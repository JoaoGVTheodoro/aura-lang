// OOP: Mixin-like Composition
class ConcreteC86261Oi {
    public def log(msg: str) { print(msg); }
}

class MockC62152Cp {
    private let logger: ConcreteC86261Oi
    public let value: int
    
    def init(val: int, logger: ConcreteC86261Oi) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC86261Oi();
let obj = MockC62152Cp(val: 50, logger: logger);
print(obj.process());
