// OOP: Delegation
class ConcreteC23632El {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class RealC60840Av {
    private let delegate: ConcreteC23632El
    
    def init(t: ConcreteC23632El) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC23632El();
target_obj.data = 25;
let delegator_obj = RealC60840Av(t: target_obj);
print(delegator_obj.process());
