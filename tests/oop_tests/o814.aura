// OOP: Static Members
class C37128Jj {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C37128Jj.instance_count = C37128Jj.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C37128Jj.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C37128Jj(id: 1);
let obj2 = C37128Jj(id: 2);
let obj3 = C37128Jj(id: 3);

print(C37128Jj.instance_count);
print(obj1.get_id());
print(obj3.get_id());
