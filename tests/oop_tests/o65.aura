// OOP: Constructor Variants
class BaseC96820Sr {
    public let value: int
    public let name: str = "default"
    
    def init(value: int) {
        self.value = value;
    }
    
    def init_with_name(value: int, name: str) {
        self.value = value;
        self.name = name;
    }
}

let obj1 = BaseC96820Sr(value: 42);
let obj2 = BaseC96820Sr(value: 100);
obj2.name = "custom";

print(obj1.value);
print(obj2.value);
print(obj2.name);
