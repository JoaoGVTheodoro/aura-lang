// OOP: Static Members
class RealC34971Xn {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC34971Xn.instance_count = RealC34971Xn.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC34971Xn.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC34971Xn(id: 1);
let obj2 = RealC34971Xn(id: 2);
let obj3 = RealC34971Xn(id: 3);

print(RealC34971Xn.instance_count);
print(obj1.get_id());
print(obj3.get_id());
