// OOP: Static Factory Pattern
class C29618Ee {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C29618Ee {
        return C29618Ee(value: 42);
    }
    
    public static def create_custom(val: int) -> C29618Ee {
        return C29618Ee(value: val);
    }
}

let obj1 = C29618Ee.create_default();
let obj2 = C29618Ee.create_custom(23);

print(obj1.get_value());
print(obj2.get_value());
