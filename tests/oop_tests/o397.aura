// OOP: Static Factory Pattern
class ConcreteC55264Os {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ConcreteC55264Os {
        return ConcreteC55264Os(value: 42);
    }
    
    public static def create_custom(val: int) -> ConcreteC55264Os {
        return ConcreteC55264Os(value: val);
    }
}

let obj1 = ConcreteC55264Os.create_default();
let obj2 = ConcreteC55264Os.create_custom(23);

print(obj1.get_value());
print(obj2.get_value());
