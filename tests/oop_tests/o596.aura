// OOP: Mixin-like Composition
class ImplC55404Fv {
    public def log(msg: str) { print(msg); }
}

class BaseC44378Th {
    private let logger: ImplC55404Fv
    public let value: int
    
    def init(val: int, logger: ImplC55404Fv) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC55404Fv();
let obj = BaseC44378Th(val: 50, logger: logger);
print(obj.process());
