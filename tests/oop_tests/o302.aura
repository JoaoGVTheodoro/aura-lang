// OOP: Mixin-like Composition
class RealC65147Fx {
    public def log(msg: str) { print(msg); }
}

class BaseC13307Sf {
    private let logger: RealC65147Fx
    public let value: int
    
    def init(val: int, logger: RealC65147Fx) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC65147Fx();
let obj = BaseC13307Sf(val: 50, logger: logger);
print(obj.process());
