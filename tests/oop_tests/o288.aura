// OOP: Mixin-like Composition
class ConcreteC29566Wk {
    public def log(msg: str) { print(msg); }
}

class C50122Th {
    private let logger: ConcreteC29566Wk
    public let value: int
    
    def init(val: int, logger: ConcreteC29566Wk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC29566Wk();
let obj = C50122Th(val: 50, logger: logger);
print(obj.process());
