// OOP: Static Factory Pattern
class AbstractC36867Qu {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC36867Qu {
        return AbstractC36867Qu(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC36867Qu {
        return AbstractC36867Qu(value: val);
    }
}

let obj1 = AbstractC36867Qu.create_default();
let obj2 = AbstractC36867Qu.create_custom(73);

print(obj1.get_value());
print(obj2.get_value());
