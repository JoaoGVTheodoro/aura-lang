// OOP: Mixin-like Composition
class RealC73393Db {
    public def log(msg: str) { print(msg); }
}

class ConcreteC34778Sv {
    private let logger: RealC73393Db
    public let value: int
    
    def init(val: int, logger: RealC73393Db) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC73393Db();
let obj = ConcreteC34778Sv(val: 50, logger: logger);
print(obj.process());
