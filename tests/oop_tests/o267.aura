// OOP: Deep Inheritance Chain (4 levels)
class MockC88040Jp {
    protected let base_val: int = 41
    public def get_val() -> int { return self.base_val; }
}

class BaseC55935Pr extends MockC88040Jp {
    public def get_val() -> int { return self.base_val * 2; }
}

class BaseC45840Fk extends BaseC55935Pr {
    public def get_val() -> int { return self.base_val * 3; }
}

class AbstractC17472Qp extends BaseC45840Fk {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = AbstractC17472Qp(base_val: 41);
print(obj.get_val());
