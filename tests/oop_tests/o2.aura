// OOP: Static Members
class AbstractC53866Gd {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC53866Gd.instance_count = AbstractC53866Gd.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC53866Gd.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC53866Gd(id: 1);
let obj2 = AbstractC53866Gd(id: 2);
let obj3 = AbstractC53866Gd(id: 3);

print(AbstractC53866Gd.instance_count);
print(obj1.get_id());
print(obj3.get_id());
