// OOP: Static Factory Pattern
class BaseC54208Hq {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC54208Hq {
        return BaseC54208Hq(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC54208Hq {
        return BaseC54208Hq(value: val);
    }
}

let obj1 = BaseC54208Hq.create_default();
let obj2 = BaseC54208Hq.create_custom(15);

print(obj1.get_value());
print(obj2.get_value());
