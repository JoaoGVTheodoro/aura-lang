// OOP: Mixin-like Composition
class ConcreteC55086Cj {
    public def log(msg: str) { print(msg); }
}

class BaseC68929Hr {
    private let logger: ConcreteC55086Cj
    public let value: int
    
    def init(val: int, logger: ConcreteC55086Cj) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC55086Cj();
let obj = BaseC68929Hr(val: 50, logger: logger);
print(obj.process());
