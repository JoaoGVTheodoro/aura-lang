// OOP: Mixin-like Composition
class BaseC80713Qr {
    public def log(msg: str) { print(msg); }
}

class AbstractC15862Yd {
    private let logger: BaseC80713Qr
    public let value: int
    
    def init(val: int, logger: BaseC80713Qr) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC80713Qr();
let obj = AbstractC15862Yd(val: 50, logger: logger);
print(obj.process());
