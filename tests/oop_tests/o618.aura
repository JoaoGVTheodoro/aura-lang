// OOP: Static Members
class C84614Ba {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C84614Ba.instance_count = C84614Ba.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C84614Ba.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C84614Ba(id: 1);
let obj2 = C84614Ba(id: 2);
let obj3 = C84614Ba(id: 3);

print(C84614Ba.instance_count);
print(obj1.get_id());
print(obj3.get_id());
