// OOP: Static Factory Pattern
class C77558Or {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C77558Or {
        return C77558Or(value: 42);
    }
    
    public static def create_custom(val: int) -> C77558Or {
        return C77558Or(value: val);
    }
}

let obj1 = C77558Or.create_default();
let obj2 = C77558Or.create_custom(8);

print(obj1.get_value());
print(obj2.get_value());
