// OOP: Composition
class AbstractC83723Ht {
    public let data: int = 20
    public def process() -> int { return self.data * 2; }
}

class MockC25013Xz {
    private let component: AbstractC83723Ht
    
    def init(comp: AbstractC83723Ht) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = AbstractC83723Ht(data: 20);
let outer_obj = MockC25013Xz(comp: inner_obj);
print(outer_obj.transform());
