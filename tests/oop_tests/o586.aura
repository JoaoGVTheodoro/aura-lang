// OOP: Delegation
class C18095Ft {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC17152Gl {
    private let delegate: C18095Ft
    
    def init(t: C18095Ft) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = C18095Ft();
target_obj.data = 25;
let delegator_obj = BaseC17152Gl(t: target_obj);
print(delegator_obj.process());
