// OOP: Static Factory Pattern
class BaseC99993Yi {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC99993Yi {
        return BaseC99993Yi(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC99993Yi {
        return BaseC99993Yi(value: val);
    }
}

let obj1 = BaseC99993Yi.create_default();
let obj2 = BaseC99993Yi.create_custom(54);

print(obj1.get_value());
print(obj2.get_value());
