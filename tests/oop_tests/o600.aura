// OOP: Delegation
class ImplC95878Tx {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class MockC80102Jm {
    private let delegate: ImplC95878Tx
    
    def init(t: ImplC95878Tx) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ImplC95878Tx();
target_obj.data = 25;
let delegator_obj = MockC80102Jm(t: target_obj);
print(delegator_obj.process());
