// OOP: Mixin-like Composition
class ImplC62455Mw {
    public def log(msg: str) { print(msg); }
}

class ConcreteC11890Ay {
    private let logger: ImplC62455Mw
    public let value: int
    
    def init(val: int, logger: ImplC62455Mw) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC62455Mw();
let obj = ConcreteC11890Ay(val: 50, logger: logger);
print(obj.process());
