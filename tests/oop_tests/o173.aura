// OOP: Static Factory Pattern
class MockC49535Sk {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC49535Sk {
        return MockC49535Sk(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC49535Sk {
        return MockC49535Sk(value: val);
    }
}

let obj1 = MockC49535Sk.create_default();
let obj2 = MockC49535Sk.create_custom(73);

print(obj1.get_value());
print(obj2.get_value());
