// OOP: Composition
class MockC72648Ma {
    public let data: int = 39
    public def process() -> int { return self.data * 2; }
}

class C98234Cs {
    private let component: MockC72648Ma
    
    def init(comp: MockC72648Ma) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = MockC72648Ma(data: 39);
let outer_obj = C98234Cs(comp: inner_obj);
print(outer_obj.transform());
