// OOP: Composition
class C20111Pu {
    public let data: int = 76
    public def process() -> int { return self.data * 2; }
}

class C89952Ra {
    private let component: C20111Pu
    
    def init(comp: C20111Pu) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C20111Pu(data: 76);
let outer_obj = C89952Ra(comp: inner_obj);
print(outer_obj.transform());
