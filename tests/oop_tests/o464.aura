// OOP: Static Members
class BaseC24958Mb {
    public static let instance_count: int = 0
    public static def increment_count() -> void { BaseC24958Mb.instance_count = BaseC24958Mb.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        BaseC24958Mb.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = BaseC24958Mb(id: 1);
let obj2 = BaseC24958Mb(id: 2);
let obj3 = BaseC24958Mb(id: 3);

print(BaseC24958Mb.instance_count);
print(obj1.get_id());
print(obj3.get_id());
