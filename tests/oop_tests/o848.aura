// OOP: Mixin-like Composition
class BaseC54357Yj {
    public def log(msg: str) { print(msg); }
}

class ConcreteC13019Cm {
    private let logger: BaseC54357Yj
    public let value: int
    
    def init(val: int, logger: BaseC54357Yj) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC54357Yj();
let obj = ConcreteC13019Cm(val: 50, logger: logger);
print(obj.process());
