// OOP: Delegation
class BaseC27212Ba {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC27671Gk {
    private let delegate: BaseC27212Ba
    
    def init(t: BaseC27212Ba) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC27212Ba();
target_obj.data = 25;
let delegator_obj = BaseC27671Gk(t: target_obj);
print(delegator_obj.process());
