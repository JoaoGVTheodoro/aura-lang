// OOP: Mixin-like Composition
class RealC87671Rj {
    public def log(msg: str) { print(msg); }
}

class ConcreteC78200Xg {
    private let logger: RealC87671Rj
    public let value: int
    
    def init(val: int, logger: RealC87671Rj) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC87671Rj();
let obj = ConcreteC78200Xg(val: 50, logger: logger);
print(obj.process());
