// OOP: Static Members
class AbstractC89384Cu {
    public static let instance_count: int = 0
    public static def increment_count() -> void { AbstractC89384Cu.instance_count = AbstractC89384Cu.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        AbstractC89384Cu.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = AbstractC89384Cu(id: 1);
let obj2 = AbstractC89384Cu(id: 2);
let obj3 = AbstractC89384Cu(id: 3);

print(AbstractC89384Cu.instance_count);
print(obj1.get_id());
print(obj3.get_id());
