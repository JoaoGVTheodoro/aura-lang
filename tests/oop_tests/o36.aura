// OOP: Mixin-like Composition
class C84252Ob {
    public def log(msg: str) { print(msg); }
}

class BaseC27820Yl {
    private let logger: C84252Ob
    public let value: int
    
    def init(val: int, logger: C84252Ob) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C84252Ob();
let obj = BaseC27820Yl(val: 50, logger: logger);
print(obj.process());
