// OOP: Static Members
class C35090Ts {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C35090Ts.instance_count = C35090Ts.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C35090Ts.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C35090Ts(id: 1);
let obj2 = C35090Ts(id: 2);
let obj3 = C35090Ts(id: 3);

print(C35090Ts.instance_count);
print(obj1.get_id());
print(obj3.get_id());
