// OOP: Mixin-like Composition
class ImplC88778Qb {
    public def log(msg: str) { print(msg); }
}

class MockC51317Tb {
    private let logger: ImplC88778Qb
    public let value: int
    
    def init(val: int, logger: ImplC88778Qb) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC88778Qb();
let obj = MockC51317Tb(val: 50, logger: logger);
print(obj.process());
