// OOP: Delegation
class ImplC37317Wt {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class BaseC46275Hk {
    private let delegate: ImplC37317Wt
    
    def init(t: ImplC37317Wt) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ImplC37317Wt();
target_obj.data = 25;
let delegator_obj = BaseC46275Hk(t: target_obj);
print(delegator_obj.process());
