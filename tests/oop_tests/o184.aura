// OOP: Static Members
class C49364Ci {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C49364Ci.instance_count = C49364Ci.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C49364Ci.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C49364Ci(id: 1);
let obj2 = C49364Ci(id: 2);
let obj3 = C49364Ci(id: 3);

print(C49364Ci.instance_count);
print(obj1.get_id());
print(obj3.get_id());
