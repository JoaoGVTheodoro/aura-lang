// OOP: Static Members
class C17169Vn {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C17169Vn.instance_count = C17169Vn.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C17169Vn.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C17169Vn(id: 1);
let obj2 = C17169Vn(id: 2);
let obj3 = C17169Vn(id: 3);

print(C17169Vn.instance_count);
print(obj1.get_id());
print(obj3.get_id());
