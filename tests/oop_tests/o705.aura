// OOP: Static Factory Pattern
class MockC45654Ez {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC45654Ez {
        return MockC45654Ez(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC45654Ez {
        return MockC45654Ez(value: val);
    }
}

let obj1 = MockC45654Ez.create_default();
let obj2 = MockC45654Ez.create_custom(56);

print(obj1.get_value());
print(obj2.get_value());
