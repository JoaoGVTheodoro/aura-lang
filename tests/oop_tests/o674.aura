// OOP: Static Members
class RealC91947Xy {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC91947Xy.instance_count = RealC91947Xy.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC91947Xy.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC91947Xy(id: 1);
let obj2 = RealC91947Xy(id: 2);
let obj3 = RealC91947Xy(id: 3);

print(RealC91947Xy.instance_count);
print(obj1.get_id());
print(obj3.get_id());
