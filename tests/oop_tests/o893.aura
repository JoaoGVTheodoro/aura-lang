// OOP: Composition
class ImplC67635Fp {
    public let data: int = 100
    public def process() -> int { return self.data * 2; }
}

class RealC25709My {
    private let component: ImplC67635Fp
    
    def init(comp: ImplC67635Fp) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = ImplC67635Fp(data: 100);
let outer_obj = RealC25709My(comp: inner_obj);
print(outer_obj.transform());
