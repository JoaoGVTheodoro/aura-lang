    protected let base_val: int = 3
    public def get_val() -> int { return self.base_val; }
}

class C93277Zw extends ImplC25502Yx {
    public def get_val() -> int { return self.base_val * 2; }
}

class ImplC23993Xo extends C93277Zw {
    public def get_val() -> int { return self.base_val * 3; }
}

let obj = ImplC23993Xo(base_val: 3);
print(obj.get_val());
