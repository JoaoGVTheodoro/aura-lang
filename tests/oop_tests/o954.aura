// OOP: Static Members
class ConcreteC41749Wa {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC41749Wa.instance_count = ConcreteC41749Wa.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC41749Wa.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC41749Wa(id: 1);
let obj2 = ConcreteC41749Wa(id: 2);
let obj3 = ConcreteC41749Wa(id: 3);

print(ConcreteC41749Wa.instance_count);
print(obj1.get_id());
print(obj3.get_id());
