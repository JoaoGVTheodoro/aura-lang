// OOP: Builder Pattern
class ImplC13006XhBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ImplC13006XhBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ImplC13006XhBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ImplC13006Xh {
        return ImplC13006Xh(name: self.name, age: self.age, email: self.email);
    }
}

class ImplC13006Xh {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ImplC13006XhBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
