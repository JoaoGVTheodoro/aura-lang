// OOP: Mixin-like Composition
class C52808Hp {
    public def log(msg: str) { print(msg); }
}

class MockC62023Nf {
    private let logger: C52808Hp
    public let value: int
    
    def init(val: int, logger: C52808Hp) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C52808Hp();
let obj = MockC62023Nf(val: 50, logger: logger);
print(obj.process());
