// OOP: Static Members
class RealC49885Cx {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC49885Cx.instance_count = RealC49885Cx.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC49885Cx.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC49885Cx(id: 1);
let obj2 = RealC49885Cx(id: 2);
let obj3 = RealC49885Cx(id: 3);

print(RealC49885Cx.instance_count);
print(obj1.get_id());
print(obj3.get_id());
