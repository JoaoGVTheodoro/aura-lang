// OOP: Static Factory Pattern
class ImplC34222Nb {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC34222Nb {
        return ImplC34222Nb(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC34222Nb {
        return ImplC34222Nb(value: val);
    }
}

let obj1 = ImplC34222Nb.create_default();
let obj2 = ImplC34222Nb.create_custom(40);

print(obj1.get_value());
print(obj2.get_value());
