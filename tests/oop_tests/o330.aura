// OOP: Mixin-like Composition
class BaseC20613Ll {
    public def log(msg: str) { print(msg); }
}

class ConcreteC55921Gb {
    private let logger: BaseC20613Ll
    public let value: int
    
    def init(val: int, logger: BaseC20613Ll) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC20613Ll();
let obj = ConcreteC55921Gb(val: 50, logger: logger);
print(obj.process());
