// OOP: Static Members
class C60827Nl {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C60827Nl.instance_count = C60827Nl.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C60827Nl.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C60827Nl(id: 1);
let obj2 = C60827Nl(id: 2);
let obj3 = C60827Nl(id: 3);

print(C60827Nl.instance_count);
print(obj1.get_id());
print(obj3.get_id());
