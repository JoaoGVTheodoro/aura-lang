// OOP: Mixin-like Composition
class C90887Tr {
    public def log(msg: str) { print(msg); }
}

class MockC39582Nh {
    private let logger: C90887Tr
    public let value: int
    
    def init(val: int, logger: C90887Tr) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C90887Tr();
let obj = MockC39582Nh(val: 50, logger: logger);
print(obj.process());
