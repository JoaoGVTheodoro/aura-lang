// OOP: Static Factory Pattern
class RealC24137Zp {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC24137Zp {
        return RealC24137Zp(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC24137Zp {
        return RealC24137Zp(value: val);
    }
}

let obj1 = RealC24137Zp.create_default();
let obj2 = RealC24137Zp.create_custom(42);

print(obj1.get_value());
print(obj2.get_value());
