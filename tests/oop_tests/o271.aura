// OOP: Static Factory Pattern
class C58903Tl {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C58903Tl {
        return C58903Tl(value: 42);
    }
    
    public static def create_custom(val: int) -> C58903Tl {
        return C58903Tl(value: val);
    }
}

let obj1 = C58903Tl.create_default();
let obj2 = C58903Tl.create_custom(99);

print(obj1.get_value());
print(obj2.get_value());
