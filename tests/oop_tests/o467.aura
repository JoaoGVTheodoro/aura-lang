// OOP: Static Factory Pattern
class BaseC19394Kl {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC19394Kl {
        return BaseC19394Kl(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC19394Kl {
        return BaseC19394Kl(value: val);
    }
}

let obj1 = BaseC19394Kl.create_default();
let obj2 = BaseC19394Kl.create_custom(90);

print(obj1.get_value());
print(obj2.get_value());
