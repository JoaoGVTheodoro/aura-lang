// OOP: Mixin-like Composition
class RealC14600Ie {
    public def log(msg: str) { print(msg); }
}

class BaseC80537Ad {
    private let logger: RealC14600Ie
    public let value: int
    
    def init(val: int, logger: RealC14600Ie) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC14600Ie();
let obj = BaseC80537Ad(val: 50, logger: logger);
print(obj.process());
