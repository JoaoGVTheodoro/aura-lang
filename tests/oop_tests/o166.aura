// OOP: Delegation
class ConcreteC21635Df {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class AbstractC13204Wl {
    private let delegate: ConcreteC21635Df
    
    def init(t: ConcreteC21635Df) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC21635Df();
target_obj.data = 25;
let delegator_obj = AbstractC13204Wl(t: target_obj);
print(delegator_obj.process());
