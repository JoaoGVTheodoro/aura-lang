// OOP: Mixin-like Composition
class AbstractC41869Hr {
    public def log(msg: str) { print(msg); }
}

class AbstractC63070Wj {
    private let logger: AbstractC41869Hr
    public let value: int
    
    def init(val: int, logger: AbstractC41869Hr) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = AbstractC41869Hr();
let obj = AbstractC63070Wj(val: 50, logger: logger);
print(obj.process());
