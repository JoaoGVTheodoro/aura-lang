// OOP: Static Factory Pattern
class AbstractC12018Ng {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC12018Ng {
        return AbstractC12018Ng(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC12018Ng {
        return AbstractC12018Ng(value: val);
    }
}

let obj1 = AbstractC12018Ng.create_default();
let obj2 = AbstractC12018Ng.create_custom(8);

print(obj1.get_value());
print(obj2.get_value());
