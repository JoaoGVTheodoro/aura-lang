// OOP: Static Members
class ConcreteC99203Jo {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC99203Jo.instance_count = ConcreteC99203Jo.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC99203Jo.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC99203Jo(id: 1);
let obj2 = ConcreteC99203Jo(id: 2);
let obj3 = ConcreteC99203Jo(id: 3);

print(ConcreteC99203Jo.instance_count);
print(obj1.get_id());
print(obj3.get_id());
