// OOP: Static Factory Pattern
class ImplC34112Vy {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC34112Vy {
        return ImplC34112Vy(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC34112Vy {
        return ImplC34112Vy(value: val);
    }
}

let obj1 = ImplC34112Vy.create_default();
let obj2 = ImplC34112Vy.create_custom(56);

print(obj1.get_value());
print(obj2.get_value());
