// OOP: Static Members
class C40823Wf {
    public static let instance_count: int = 0
    public static def increment_count() -> void { C40823Wf.instance_count = C40823Wf.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        C40823Wf.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = C40823Wf(id: 1);
let obj2 = C40823Wf(id: 2);
let obj3 = C40823Wf(id: 3);

print(C40823Wf.instance_count);
print(obj1.get_id());
print(obj3.get_id());
