// OOP: Mixin-like Composition
class ImplC67904Lu {
    public def log(msg: str) { print(msg); }
}

class C80391Aw {
    private let logger: ImplC67904Lu
    public let value: int
    
    def init(val: int, logger: ImplC67904Lu) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC67904Lu();
let obj = C80391Aw(val: 50, logger: logger);
print(obj.process());
