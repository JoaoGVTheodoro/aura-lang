// OOP: Composition
class C74070Rw {
    public let data: int = 28
    public def process() -> int { return self.data * 2; }
}

class ConcreteC31534Oi {
    private let component: C74070Rw
    
    def init(comp: C74070Rw) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = C74070Rw(data: 28);
let outer_obj = ConcreteC31534Oi(comp: inner_obj);
print(outer_obj.transform());
