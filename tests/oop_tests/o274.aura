// OOP: Mixin-like Composition
class RealC25653Zt {
    public def log(msg: str) { print(msg); }
}

class C96952Hu {
    private let logger: RealC25653Zt
    public let value: int
    
    def init(val: int, logger: RealC25653Zt) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = RealC25653Zt();
let obj = C96952Hu(val: 50, logger: logger);
print(obj.process());
