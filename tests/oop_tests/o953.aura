// OOP: Deep Inheritance Chain (4 levels)
class MockC27042Wf {
    protected let base_val: int = 9
    public def get_val() -> int { return self.base_val; }
}

class AbstractC50180Ji extends MockC27042Wf {
    public def get_val() -> int { return self.base_val * 2; }
}

class ConcreteC45162Ug extends AbstractC50180Ji {
    public def get_val() -> int { return self.base_val * 3; }
}

class RealC98290Jj extends ConcreteC45162Ug {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = RealC98290Jj(base_val: 9);
print(obj.get_val());
