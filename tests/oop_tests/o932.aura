// OOP: Mixin-like Composition
class ConcreteC53711Gp {
    public def log(msg: str) { print(msg); }
}

class MockC10187Mb {
    private let logger: ConcreteC53711Gp
    public let value: int
    
    def init(val: int, logger: ConcreteC53711Gp) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ConcreteC53711Gp();
let obj = MockC10187Mb(val: 50, logger: logger);
print(obj.process());
