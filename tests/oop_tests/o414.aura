// OOP: Mixin-like Composition
class BaseC77208Dd {
    public def log(msg: str) { print(msg); }
}

class C87474Ik {
    private let logger: BaseC77208Dd
    public let value: int
    
    def init(val: int, logger: BaseC77208Dd) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = BaseC77208Dd();
let obj = C87474Ik(val: 50, logger: logger);
print(obj.process());
