// OOP: Static Factory Pattern
class C10223Un {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C10223Un {
        return C10223Un(value: 42);
    }
    
    public static def create_custom(val: int) -> C10223Un {
        return C10223Un(value: val);
    }
}

let obj1 = C10223Un.create_default();
let obj2 = C10223Un.create_custom(46);

print(obj1.get_value());
print(obj2.get_value());
