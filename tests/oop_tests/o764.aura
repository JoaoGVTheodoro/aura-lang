// OOP: Mixin-like Composition
class AbstractC99630Op {
    public def log(msg: str) { print(msg); }
}

class ImplC65662Yr {
    private let logger: AbstractC99630Op
    public let value: int
    
    def init(val: int, logger: AbstractC99630Op) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = AbstractC99630Op();
let obj = ImplC65662Yr(val: 50, logger: logger);
print(obj.process());
