// OOP: Static Factory Pattern
class ImplC88956Ho {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ImplC88956Ho {
        return ImplC88956Ho(value: 42);
    }
    
    public static def create_custom(val: int) -> ImplC88956Ho {
        return ImplC88956Ho(value: val);
    }
}

let obj1 = ImplC88956Ho.create_default();
let obj2 = ImplC88956Ho.create_custom(84);

print(obj1.get_value());
print(obj2.get_value());
