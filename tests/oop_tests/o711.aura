// OOP: Composition
class RealC59214Tj {
    public let data: int = 47
    public def process() -> int { return self.data * 2; }
}

class C16847Hc {
    private let component: RealC59214Tj
    
    def init(comp: RealC59214Tj) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC59214Tj(data: 47);
let outer_obj = C16847Hc(comp: inner_obj);
print(outer_obj.transform());
