// OOP: Static Factory Pattern
class BaseC64959Jt {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> BaseC64959Jt {
        return BaseC64959Jt(value: 42);
    }
    
    public static def create_custom(val: int) -> BaseC64959Jt {
        return BaseC64959Jt(value: val);
    }
}

let obj1 = BaseC64959Jt.create_default();
let obj2 = BaseC64959Jt.create_custom(42);

print(obj1.get_value());
print(obj2.get_value());
