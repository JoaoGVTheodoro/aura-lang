// OOP: Composition
class BaseC93172Nm {
    public let data: int = 34
    public def process() -> int { return self.data * 2; }
}

class MockC49988Sp {
    private let component: BaseC93172Nm
    
    def init(comp: BaseC93172Nm) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC93172Nm(data: 34);
let outer_obj = MockC49988Sp(comp: inner_obj);
print(outer_obj.transform());
