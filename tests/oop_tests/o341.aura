// OOP: Static Factory Pattern
class AbstractC42736Dv {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC42736Dv {
        return AbstractC42736Dv(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC42736Dv {
        return AbstractC42736Dv(value: val);
    }
}

let obj1 = AbstractC42736Dv.create_default();
let obj2 = AbstractC42736Dv.create_custom(87);

print(obj1.get_value());
print(obj2.get_value());
