// OOP: Composition
class RealC82213Zy {
    public let data: int = 43
    public def process() -> int { return self.data * 2; }
}

class RealC37700Ex {
    private let component: RealC82213Zy
    
    def init(comp: RealC82213Zy) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = RealC82213Zy(data: 43);
let outer_obj = RealC37700Ex(comp: inner_obj);
print(outer_obj.transform());
