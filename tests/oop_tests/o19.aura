// OOP: Static Factory Pattern
class MockC38641Vx {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC38641Vx {
        return MockC38641Vx(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC38641Vx {
        return MockC38641Vx(value: val);
    }
}

let obj1 = MockC38641Vx.create_default();
let obj2 = MockC38641Vx.create_custom(76);

print(obj1.get_value());
print(obj2.get_value());
