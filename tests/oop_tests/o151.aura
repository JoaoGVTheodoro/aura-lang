// OOP: Composition
class BaseC65149Zn {
    public let data: int = 59
    public def process() -> int { return self.data * 2; }
}

class ConcreteC76627Dj {
    private let component: BaseC65149Zn
    
    def init(comp: BaseC65149Zn) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = BaseC65149Zn(data: 59);
let outer_obj = ConcreteC76627Dj(comp: inner_obj);
print(outer_obj.transform());
