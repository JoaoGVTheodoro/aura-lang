// OOP: Builder Pattern
class ImplC54305SrBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> ImplC54305SrBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> ImplC54305SrBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> ImplC54305Sr {
        return ImplC54305Sr(name: self.name, age: self.age, email: self.email);
    }
}

class ImplC54305Sr {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = ImplC54305SrBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
