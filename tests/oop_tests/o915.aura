// OOP: Static Factory Pattern
class AbstractC72679En {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC72679En {
        return AbstractC72679En(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC72679En {
        return AbstractC72679En(value: val);
    }
}

let obj1 = AbstractC72679En.create_default();
let obj2 = AbstractC72679En.create_custom(80);

print(obj1.get_value());
print(obj2.get_value());
