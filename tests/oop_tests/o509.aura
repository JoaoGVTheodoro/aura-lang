// OOP: Static Factory Pattern
class ConcreteC99069Zu {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> ConcreteC99069Zu {
        return ConcreteC99069Zu(value: 42);
    }
    
    public static def create_custom(val: int) -> ConcreteC99069Zu {
        return ConcreteC99069Zu(value: val);
    }
}

let obj1 = ConcreteC99069Zu.create_default();
let obj2 = ConcreteC99069Zu.create_custom(91);

print(obj1.get_value());
print(obj2.get_value());
