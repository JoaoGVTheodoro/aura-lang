// OOP: Static Factory Pattern
class RealC13062Ds {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC13062Ds {
        return RealC13062Ds(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC13062Ds {
        return RealC13062Ds(value: val);
    }
}

let obj1 = RealC13062Ds.create_default();
let obj2 = RealC13062Ds.create_custom(41);

print(obj1.get_value());
print(obj2.get_value());
