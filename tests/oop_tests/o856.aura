// OOP: Static Members
class RealC33633Kq {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC33633Kq.instance_count = RealC33633Kq.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC33633Kq.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC33633Kq(id: 1);
let obj2 = RealC33633Kq(id: 2);
let obj3 = RealC33633Kq(id: 3);

print(RealC33633Kq.instance_count);
print(obj1.get_id());
print(obj3.get_id());
