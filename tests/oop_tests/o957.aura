// OOP: Static Factory Pattern
class RealC34768Jl {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> RealC34768Jl {
        return RealC34768Jl(value: 42);
    }
    
    public static def create_custom(val: int) -> RealC34768Jl {
        return RealC34768Jl(value: val);
    }
}

let obj1 = RealC34768Jl.create_default();
let obj2 = RealC34768Jl.create_custom(30);

print(obj1.get_value());
print(obj2.get_value());
