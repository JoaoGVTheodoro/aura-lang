// OOP: Static Members
class RealC51670Rx {
    public static let instance_count: int = 0
    public static def increment_count() -> void { RealC51670Rx.instance_count = RealC51670Rx.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        RealC51670Rx.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = RealC51670Rx(id: 1);
let obj2 = RealC51670Rx(id: 2);
let obj3 = RealC51670Rx(id: 3);

print(RealC51670Rx.instance_count);
print(obj1.get_id());
print(obj3.get_id());
