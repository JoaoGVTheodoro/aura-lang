// OOP: Composition
class AbstractC75981Dm {
    public let data: int = 37
    public def process() -> int { return self.data * 2; }
}

class BaseC77581Wu {
    private let component: AbstractC75981Dm
    
    def init(comp: AbstractC75981Dm) {
        self.component = comp;
    }
    
    public def transform() -> int {
        return self.component.process() + 10;
    }
}

let inner_obj = AbstractC75981Dm(data: 37);
let outer_obj = BaseC77581Wu(comp: inner_obj);
print(outer_obj.transform());
