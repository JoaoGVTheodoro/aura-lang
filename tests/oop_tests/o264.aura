// OOP: Delegation
class BaseC22247Fh {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class RealC68528Sm {
    private let delegate: BaseC22247Fh
    
    def init(t: BaseC22247Fh) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = BaseC22247Fh();
target_obj.data = 25;
let delegator_obj = RealC68528Sm(t: target_obj);
print(delegator_obj.process());
