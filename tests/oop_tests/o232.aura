// OOP: Mixin-like Composition
class ImplC35632Hg {
    public def log(msg: str) { print(msg); }
}

class RealC74178Cg {
    private let logger: ImplC35632Hg
    public let value: int
    
    def init(val: int, logger: ImplC35632Hg) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = ImplC35632Hg();
let obj = RealC74178Cg(val: 50, logger: logger);
print(obj.process());
