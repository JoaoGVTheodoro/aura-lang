// OOP: Static Factory Pattern
class MockC29682Wc {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> MockC29682Wc {
        return MockC29682Wc(value: 42);
    }
    
    public static def create_custom(val: int) -> MockC29682Wc {
        return MockC29682Wc(value: val);
    }
}

let obj1 = MockC29682Wc.create_default();
let obj2 = MockC29682Wc.create_custom(30);

print(obj1.get_value());
print(obj2.get_value());
