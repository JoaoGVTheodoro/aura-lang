// OOP: Mixin-like Composition
class MockC71105Qj {
    public def log(msg: str) { print(msg); }
}

class RealC74992Ix {
    private let logger: MockC71105Qj
    public let value: int
    
    def init(val: int, logger: MockC71105Qj) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = MockC71105Qj();
let obj = RealC74992Ix(val: 50, logger: logger);
print(obj.process());
