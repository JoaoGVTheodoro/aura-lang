// OOP: Mixin-like Composition
class C81086Vk {
    public def log(msg: str) { print(msg); }
}

class ImplC53138Fj {
    private let logger: C81086Vk
    public let value: int
    
    def init(val: int, logger: C81086Vk) {
        self.value = val;
        self.logger = logger;
    }
    
    public def process() -> int {
        self.logger.log("Processing");
        return self.value * 2;
    }
}

let logger = C81086Vk();
let obj = ImplC53138Fj(val: 50, logger: logger);
print(obj.process());
