// OOP: Static Factory Pattern
class AbstractC88343Rw {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> AbstractC88343Rw {
        return AbstractC88343Rw(value: 42);
    }
    
    public static def create_custom(val: int) -> AbstractC88343Rw {
        return AbstractC88343Rw(value: val);
    }
}

let obj1 = AbstractC88343Rw.create_default();
let obj2 = AbstractC88343Rw.create_custom(44);

print(obj1.get_value());
print(obj2.get_value());
