// OOP: Delegation
class ConcreteC48675Rx {
    public let data: int = 0
    public def process() -> int { return self.data * 2; }
}

class ImplC66697Mm {
    private let delegate: ConcreteC48675Rx
    
    def init(t: ConcreteC48675Rx) { self.delegate = t; }
    
    public def process() -> int {
        return self.delegate.process() + 10;
    }
}

let target_obj = ConcreteC48675Rx();
target_obj.data = 25;
let delegator_obj = ImplC66697Mm(t: target_obj);
print(delegator_obj.process());
