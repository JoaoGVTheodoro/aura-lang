// OOP: Static Factory Pattern
class C16260Ce {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C16260Ce {
        return C16260Ce(value: 42);
    }
    
    public static def create_custom(val: int) -> C16260Ce {
        return C16260Ce(value: val);
    }
}

let obj1 = C16260Ce.create_default();
let obj2 = C16260Ce.create_custom(14);

print(obj1.get_value());
print(obj2.get_value());
