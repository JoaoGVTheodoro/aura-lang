// OOP: Static Members
class ConcreteC58707Fv {
    public static let instance_count: int = 0
    public static def increment_count() -> void { ConcreteC58707Fv.instance_count = ConcreteC58707Fv.instance_count + 1; }
    
    public let id: int
    
    def init(id: int) {
        self.id = id;
        ConcreteC58707Fv.increment_count();
    }
    
    public def get_id() -> int { return self.id; }
}

let obj1 = ConcreteC58707Fv(id: 1);
let obj2 = ConcreteC58707Fv(id: 2);
let obj3 = ConcreteC58707Fv(id: 3);

print(ConcreteC58707Fv.instance_count);
print(obj1.get_id());
print(obj3.get_id());
