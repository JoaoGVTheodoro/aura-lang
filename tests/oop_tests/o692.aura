// OOP: Builder Pattern
class AbstractC12430WcBuilder {
    private let name: str = ""
    private let age: int = 0
    private let email: str = ""
    
    public def with_name(n: str) -> AbstractC12430WcBuilder {
        self.name = n;
        return self;
    }
    
    public def with_age(a: int) -> AbstractC12430WcBuilder {
        self.age = a;
        return self;
    }
    
    public def build() -> AbstractC12430Wc {
        return AbstractC12430Wc(name: self.name, age: self.age, email: self.email);
    }
}

class AbstractC12430Wc {
    public let name: str
    public let age: int
    public let email: str
    
    def init(name: str, age: int, email: str = "") {
        self.name = name;
        self.age = age;
        self.email = email;
    }
}

let obj = AbstractC12430WcBuilder().with_name("Aura").with_age(30).build();
print(obj.name);
print(obj.age);
