// OOP: Deep Inheritance Chain (4 levels)
class RealC36179Pv {
    protected let base_val: int = 16
    public def get_val() -> int { return self.base_val; }
}

class ConcreteC98371Px extends RealC36179Pv {
    public def get_val() -> int { return self.base_val * 2; }
}

class ConcreteC79366Ul extends ConcreteC98371Px {
    public def get_val() -> int { return self.base_val * 3; }
}

class AbstractC87346Bb extends ConcreteC79366Ul {
    public def get_val() -> int { return self.base_val * 4; }
}

let obj = AbstractC87346Bb(base_val: 16);
print(obj.get_val());
