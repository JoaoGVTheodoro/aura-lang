// OOP: Static Factory Pattern
class C42782Pn {
    private let value: int
    
    def init(value: int) { self.value = value; }
    
    public def get_value() -> int { return self.value; }
    
    public static def create_default() -> C42782Pn {
        return C42782Pn(value: 42);
    }
    
    public static def create_custom(val: int) -> C42782Pn {
        return C42782Pn(value: val);
    }
}

let obj1 = C42782Pn.create_default();
let obj2 = C42782Pn.create_custom(69);

print(obj1.get_value());
print(obj2.get_value());
