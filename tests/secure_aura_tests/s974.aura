// Security: Input Validation
def validate_31721(input: int) -> bool {
    guard input >= 0 else { return false; }
    guard input <= 100 else { return false; }
    return true;
}

let test1 = validate_31721(50);
let test2 = validate_31721(-5);
let test3 = validate_31721(101);

print(test1);
print(test2);
print(test3);
