// Security: Input Validation
def validate_10737(input: int) -> bool {
    guard input >= 0 else { return false; }
    guard input <= 100 else { return false; }
    return true;
}

let test1 = validate_10737(50);
let test2 = validate_10737(-5);
let test3 = validate_10737(101);

print(test1);
print(test2);
print(test3);
