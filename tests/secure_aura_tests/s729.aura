// Security: Input Validation
def validate_57650(input: int) -> bool {
    guard input >= 0 else { return false; }
    guard input <= 100 else { return false; }
    return true;
}

let test1 = validate_57650(50);
let test2 = validate_57650(-5);
let test3 = validate_57650(101);

print(test1);
print(test2);
print(test3);
