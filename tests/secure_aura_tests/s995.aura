// Security: Input Validation
def validate_44776(input: int) -> bool {
    guard input >= 0 else { return false; }
    guard input <= 100 else { return false; }
    return true;
}

let test1 = validate_44776(50);
let test2 = validate_44776(-5);
let test3 = validate_44776(101);

print(test1);
print(test2);
print(test3);
