// Security: Input Validation
def validate_74549(input: int) -> bool {
    guard input >= 0 else { return false; }
    guard input <= 100 else { return false; }
    return true;
}

let test1 = validate_74549(50);
let test2 = validate_74549(-5);
let test3 = validate_74549(101);

print(test1);
print(test2);
print(test3);
