// Pattern Matching with Control Flow
let value = 42;

if value < 10 {
    print("Small");
} else if value < 50 {
    print("Medium");
} else {
    print("Large");
}

print("Value: ");
print(value);

// Guard pattern
guard value > 0 else {
    print("Invalid");
}

print("Valid positive number");

// Unless pattern
unless value == 0 {
    print("Not zero");
}
