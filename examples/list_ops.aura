// List Operations
let numbers = [1, 2, 3, 4, 5];

// Filter even numbers
let evens = [];
for num in numbers {
    if num % 2 == 0 {
        evens = [*evens, num];
    }
}

print("Original: ");
print(numbers);

print("Evens: ");
print(evens);

// Double each number
let doubled = [];
for num in numbers {
    doubled = [*doubled, num * 2];
}

print("Doubled: ");
print(doubled);
