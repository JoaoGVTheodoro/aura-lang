let value = 10;
let nullable: String? = "hello";
let coalesced = nullable ?? "default";

let result = if value > 5 { "big" } else { "small" };

def safe_div(a: Float, b: Float) {
    if b == 0 {
        return null;
    }
    return a / b;
}

try {
    let x = safe_div(10, 0);
} catch {
    let error = "Division failed";
} finally {
    let cleanup = "done";
}
