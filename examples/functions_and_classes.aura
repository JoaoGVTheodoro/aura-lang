let name = "World";
let age = 42;

def greet(person: str) {
    let greeting = "Hello, " + person;
    return greeting;
}

class User {
    name: String
    age: Int
    
    def __init__(self, name: String, age: Int) {
        self.name = name;
        self.age = age;
    }
    
    def display(self) {
        let msg = self.name + " is " + self.age;
        return msg;
    }
}
